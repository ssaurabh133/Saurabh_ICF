    </action>
    <!--Code By Ritesh Ends Here -->
    
    
      <action    path="/payReportAction"  scope="request"
      name="commonReportDynaForm" attribute="commonReportDynaForm"  parameter="method"
      type="com.payout.actions.PayReportAction">
      <forward name="openJsp" path="/JSP/PAYJSP/bpListReport.jsp"></forward>
      <forward name="openTransactionSummaryJsp" path="/JSP/PAYJSP/transactionSummaryReport.jsp"></forward>
      <forward name="openPaymentSummaryReportJsp" path="/JSP/PAYJSP/paymentSummaryReport.jsp"></forward>
      </action>
