	<action-mappings >
   
       <action  input="/JSP/PAYJSP/activityMasterAdd.jsp" path="/activityMasterDispatch" parameter="method" scope="request"
         name="ActivityMasterDynaForm"      type="com.payout.actions.ActivityMasterDispatchAction">
      	
        <forward name="openAdd" path="/JSP/PAYJSP/activityMasterAdd.jsp"></forward>
         <forward name="search" path="/JSP/PAYJSP/activitySearch.jsp"></forward>
    </action>
  
    <action input="/JSP/PAYJSP/activitySearch.jsp"     path="/activityMasterBehind"  scope="request"
      name="CommonPayDynaForm" attribute="CommonPayDynaForm"     type="com.payout.actions.ActivityMasterBehindAction">
      <forward name="success" path="/JSP/PAYJSP/activitySearch.jsp"></forward>
      </action>
    
		<action input="/JSP/PAYJSP/taxMasterSearch.jsp"     path="/taxMasterBehind"  scope="request"
      name="CommonPayDynaForm" attribute="CommonPayDynaForm"     type="com.payout.actions.TaxMasterBehindAction">
      <forward name="success" path="/JSP/PAYJSP/taxMasterSearch.jsp"></forward>
