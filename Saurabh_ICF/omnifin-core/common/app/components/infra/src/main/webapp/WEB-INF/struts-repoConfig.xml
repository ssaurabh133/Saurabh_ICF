	<!-- Repo Marking marker end -->
	
	
	
	<!-- Richa Work space end -->
	
	<!-- Check List master start -->
	
		<form-bean name="repoAssetChecklistDynaValidatorForm"
			type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="assetClass" type="java.lang.String" />
			<form-property name="recStatus" type="java.lang.String" />
