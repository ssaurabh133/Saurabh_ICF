			<form-property name="assetId" type="java.lang.String" />
			<form-property name="lbxLoanIdSearch" type="java.lang.String" />
			<form-property name="searchRepoId" type="java.lang.String" />
			<form-property name="stockyardRemarks" type="java.lang.String" />
			
