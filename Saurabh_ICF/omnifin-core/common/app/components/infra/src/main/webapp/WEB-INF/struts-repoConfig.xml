	<global-exceptions />
	<global-forwards>


	</global-forwards>

	
	<action-mappings >
	
	


	<!-- vinod work space start for Repo -->
		
		<!-- Repo Detail By Agency Start-->
		
		<action path="/repoDetailbyAgencyDispatchAction" attribute="repoDetailDynaValidatorForm"
			input="/JSP/REPOJSP/repoDetailbyAgency.jsp" name="repoDetailDynaValidatorForm"
			scope="request" parameter="method" type="com.repo.actions.RepoDetailbyAgencyDispatchAction">
			<forward name="search" path="/JSP/REPOJSP/repoDetailbyAgencySearch.jsp" />
			<forward name="edit" path="/JSP/REPOJSP/repoDetailbyAgency.jsp" />
		</action>
	
		<!-- Repo Detail By Agency end-->
		
		<!-- Legal Repossesed Mis Start-->
		
		<action path="/repossessedMISDispatchAction" attribute="repossessedMISDynaForm"
			input="/JSP/REPOJSP/repossessedMis.jsp" name="repossessedMISDynaForm"
			scope="request" parameter="method" type="com.repo.actions.RepoRepossessedMisDispatchAction">
			<forward name="success" path="/JSP/REPOJSP/repossessedMis.jsp" />
		</action>
	
		<!-- Legal  Repossesed Mis end-->
		
		<!-- Legal Repossessed Reject Mis Start-->
		
		<action path="/repossessedRejectCaseMISDispatchAction" attribute="repossessedMISDynaForm"
			input="/JSP/REPOJSP/repossessedRejectCaseMis.jsp" name="repossessedMISDynaForm"
			scope="request" parameter="method" type="com.repo.actions.RepoRepossessedRejectMisDispatchAction">
			<forward name="success" path="/JSP/REPOJSP/repossessedRejectCaseMis.jsp" />
		</action>
	
		<!-- Legal  Repossessed Reject Mis end-->
	
	<!-- vinod work space end for Repo  -->
	
	<!-- Richa work space start for Repo -->
		
		<!-- Stockyard Master Start-->
		
		<action input="/JSP/REPOJSP/stockyardMasterAdd.jsp" path="/repoStockyardMasterDispatch" parameter="method" scope="request"
			name="stockyardMasterAddDyanavalidatiorForm" type="com.repo.actions.RepoStockyardMasterDispatchAction">
			<forward name="search" path="/JSP/REPOJSP/stockyardMasterSearch.jsp" />
			<forward name="edit" path="/JSP/REPOJSP/stockyardMasterAdd.jsp" />
			<forward name="editStockyard" path="/JSP/REPOJSP/stockyardMasterAdd.jsp"></forward>
		</action>
		<!-- Stockyard Master  end-->
		
		<!-- Repo Detail By Stockyar start -->
		
		<action input="/JSP/REPOJSP/repoDetailStockyardAdd.jsp" path="/repoDetailByStockyardDispatch" parameter="method" scope="request"
			name="repoDetailStockyardDyanaForm" type="com.repo.actions.RepoDetailByStockyardDispatchAction">
			<forward name="success" path="/JSP/REPOJSP/repoDetailByStockyardSearch.jsp" />
			<forward name="edit" path="/JSP/REPOJSP/repoDetailStockyardAdd.jsp" />
			<forward name="editDetail" path="/JSP/REPOJSP/repoDetailStockyardAdd.jsp"></forward>
		</action>
		
		<!--Repo Detail By Stockyar end  -->
		
		<!-- Repo Marking Maker start -->
	
		<action input="/JSP/REPOJSP/repoMarkingMakerAdd.jsp" path="/repoMarkingMakerDispatch" parameter="method" scope="request"
				name="repoMarkingMakerDynaValidatorForm" type="com.repo.actions.RepoMarkingMakerDispatchAction">
				<forward name="search" path="/JSP/REPOJSP/repoMarkingMakerSearch.jsp" />
				<forward name="edit" path="/JSP/REPOJSP/repoMarkingMakerAdd.jsp" />
				<forward name="openAdd" path="/JSP/REPOJSP/repoMarkingMakerAdd.jsp" />
		</action>	
		
		
		<action path="/repoMarkingMakerAjax" parameter="method" scope="request"
			name="repoMarkingMakerDynaValidatorForm" type="com.repo.actions.RepoMarkingMakerDispatchAction">
			<forward name="addedit" path="/JSP/REPOJSP/ajaxRepoMarkingMaker.jsp" />
		</action>
	
	<!-- Repo Marking Maker end -->
	
	<!-- Repo Marking Author start -->
	
		<action input="/JSP/REPOJSP/repoMarkingAuthorCaseAuthor.jsp" path="/repoMarkingAuthorDispatch" parameter="method" scope="request"
			name="repoMarkingMakerDynaValidatorForm" type="com.repo.actions.RepoMarkingAuthorDispatchAction">
			<forward name="success" path="/JSP/REPOJSP/repoMarkingAuthorSearch.jsp" />
			<forward name="openEditRepoMarkingAuthorCase" path="/JSP/REPOJSP/REPO_frames_RepoMarkingAuthor.jsp" />
			<forward name="noticeCheckerAuthor" path="/JSP/REPOJSP/repoMarkingAuthorCaseAuthor.jsp" />
			<forward name="authorScreen" path="/JSP/REPOJSP/repoMarkingAuthorCaseAuthor.jsp" />
			<forward name="success" path="/JSP/REPOJSP/repoMarkingAuthorSearch.jsp" />
		</action>
	
	<!-- Repo Marking Author end -->
	
	<!-- Richa work space end for Repo  -->
	
	<!-- Nazia Work space start -->
	
	<!-- Repo Asset Checklist Master Start-->
		
		<action input="/JSP/REPOJSP/repoAssetSearch.jsp" path="/RepoAssetChecklistDispatch" parameter="method" scope="request" 
		name="repoAssetChecklistDynaValidatorForm" type="com.repo.actions.RepoAssetChecklistDispatchAction">
			<forward name="search" path="/JSP/REPOJSP/repoAssetSearch.jsp" />
