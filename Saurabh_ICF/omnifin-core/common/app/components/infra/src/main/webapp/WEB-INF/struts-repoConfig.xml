			<forward name="openAdd" path="/JSP/REPOJSP/repoAssetAdd.jsp" />
			
		</action> 
		
	<!-- Repo Asset Checklist Master End-->
	
	<!-- Repo Confirmation start -->
	
		<action path="/RepoConfirmationDispatch" attribute="repoConfirmationDynaValidatorForm"
				input="/JSP/REPOJSP/repoConfirmationAuthor.jsp" name="repoConfirmationDynaValidatorForm"
				scope="request" parameter="method" type="com.repo.actions.RepoConfirmationDispatchAction">
				<forward name="search" path="/JSP/REPOJSP/repoConfirmationSearch.jsp" />
				<forward name="authorScreen" path="/JSP/REPOJSP/repoConfirmationAuthor.jsp" />
				<forward name="openEdit" path="/JSP/REPOJSP/repo_frames_repoConfirmationTab.jsp" />
				
		</action>
			
	<!-- Repo Confirmation end  -->
	
	<!--  Nazia Work space end  -->

	</action-mappings>


	<message-resources parameter="com.yourcompany.struts.ApplicationResources" />


	<plug-in className="org.apache.struts.validator.ValidatorPlugIn">
		<set-property property="pathnames"
			value="/WEB-INF/validator-rules.xml,/WEB-INF/validation.xml" />
	</plug-in>

</struts-config>

