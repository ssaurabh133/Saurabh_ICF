	<queryString>
		<![CDATA[
-- bank book customized

select com_company_m.COMPANY_DESC ,max(id.RECIPT_NO)as RECIPT_NO,
max(ifnull(date_format(if(glvh.module_id='GL', glvh.VOUCHER_DATE,ud.VALUE_DATE),'%d-%m-%Y'),''))VALUE_DATE,
temp_ledger.COMPANY_ID, temp_ledger.BRANCH_ID, temp_ledger.Ledger_ID, temp_ledger.Module_ID, temp_ledger.Product_ID, temp_ledger.Session_ID, temp_ledger.VOUCHER_DATE, replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(concat(ifnull(temp_ledger.NARRATION,''),' '),'!','! '),'%','% '),'(','( '),')',') '),'-','- '),'{','{ '),'[','[ '),'}','} '),']','] '),':',': '),';','; '),'"','" '),'/','/ '),'?','? '),'.','. '),',',', ')NARRATION , temp_ledger.VOUCHER_TYPE, concat(temp_ledger.VOUCHER_NO,' ')as VOUCHER_NO , concat(temp_ledger.cheque_invoice_no,' ') as cheque_invoice_no , replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(concat(ifnull(temp_ledger.LEDGER_DESC,''),' '),'!','! '),'%','% '),'(','( '),')',') '),'-','- '),'{','{ '),'[','[ '),'}','} '),']','] '),':',': '),';','; '),'"','" '),'/','/ '),'?','? '),'.','. '),',',', ')LEDGER_DESC, temp_ledger.DR_AMOUNT, temp_ledger.CR_AMOUNT, temp_ledger.REFERENCE_NO, replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(concat(ifnull(temp_ledger.SL_DESCRIPTION,''),' '),'!','! '),'%','% '),'(','( '),')',') '),'-','- '),'{','{ '),'[','[ '),'}','} '),']','] '),':',': '),';','; '),'"','" '),'/','/ '),'?','? '),'.','. '),',',', ')SL_DESCRIPTION, temp_ledger.SNo,temp_ledger.Header ,
if(temp_ledger.CR_AMOUNT > 0 , temp_ledger.CR_AMOUNT ,0) as Debit ,
if(temp_ledger.DR_AMOUNT > 0 , temp_ledger.DR_AMOUNT ,0) as Credit ,
if(temp_ledger.VOUCHER_NO = '' or temp_ledger.VOUCHER_NO is null , 'Opening Balance', concat(if(temp_ledger.CR_AMOUNT > 0,'To','By') , ' ' , LEDGER_DESC  )) as LedgerDescription ,
if(temp_ledger.VOUCHER_TYPE ='R','Receipt',if(temp_ledger.VOUCHER_TYPE = 'P','Payment',if(temp_ledger.VOUCHER_TYPE ='J','Journal',if(temp_ledger.VOUCHER_TYPE ='C','Contra',if(temp_ledger.VOUCHER_TYPE = 'M','Memorandum',''))))) as VoucherType ,
(select concat(trim(concat(ADDRESS_LINE1 , ' ' , ADDRESS_LINE2 , ' ' , ADDRESS_LINE3)) , ' ' , CITY , ' - ' , PINCODE ) as Address  from com_company_m) as CompanyAddress,
(select PARAMETER_VALUE from parameter_mst where PARAMETER_KEY = 'APPLICATION_PATH') as AppPah,
concat(if($P{p_headers} =$P{p_headers} ,concat( $P{p_heading}  , Ledger_Desc , ' ' , temp_ledger.Ledger_id),concat($P{p_heading}  , SL_DESCRIPTION , ' (' , Ledger_Desc , ' ' , temp_ledger.Ledger_id, ')'  )) , '\n' , Header) as Headers,(Select Ledger_Desc from temp_ledger  where  temp_ledger.Session_ID = $P{p_session_id}
order by sno,temp_ledger.VOUCHER_DATE  , mid(temp_ledger.VOUCHER_NO,2) limit 1 ) as mainled
from temp_ledger
inner join com_company_m on temp_ledger.COMPANY_ID = com_company_m.COMPANY_ID
left join cr_loan_voucher_dtl vd on temp_ledger.voucher_no=vd.voucher_no
 left join gl_voucher_dtl glvh on glvh.VOUCHER_NO=temp_ledger.VOUCHER_NO
left join cr_instrument_dtl id on id.txnid=vd.loan_id and id.instrument_id=vd.txn_id
left join cr_instrument_update_dtl ud on ud.instrument_id=id.instrument_id    and if(vd.TXN_TYPE = 'CB'  ,ud.VALUE_DATE = temp_ledger.VOUCHER_DATE,'a'='a')
and  case when vd.TXN_TYPE = 'CD' then ud.next_status in ('R','B')
         when vd.TXN_TYPE = 'CB' then ud.next_status ='B'
         when vd.TXN_TYPE = 'CDX' then ud.next_status ='X'
	 when vd.TXN_TYPE = 'CP' then ud.next_status ='R'
         when vd.TXN_TYPE = 'CPX' then ud.next_status ='X'   END

and (vd.TXN_TYPE in ('CB', 'CDX', 'CPX') OR (vd.TXN_TYPE in ('CD', 'CP') and instrument_batch_id = (select max(ud1.instrument_batch_id) from cr_instrument_update_dtl ud1 where ud1.instrument_id = ud.instrument_id
	and  ud1.next_status in ('R','B')))) and ud.REC_STATUS='R'

where  temp_ledger.Session_ID = $P{p_session_id}  --  AND temp_ledger.VOUCHER_NO IN ('R2014061700000005', 'R2014061700000012')
 group by sno,VOUCHER_DATE  , mid(temp_ledger.VOUCHER_NO,2),temp_ledger.Ledger_ID
order by sno,VOUCHER_DATE  , mid(temp_ledger.VOUCHER_NO,2) ;]]>
