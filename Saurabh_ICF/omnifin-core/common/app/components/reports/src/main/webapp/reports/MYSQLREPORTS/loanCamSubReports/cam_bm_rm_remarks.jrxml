	<queryString>
		<![CDATA[SELECT DISTINCT if(B.AUTHOR_DATE='0000-00-00 00:00:00',null,B.AUTHOR_DATE) AS DECISION_DATE,CONCAT(C.USER_NAME,' ') AS DECISION_BY,
CASE WHEN A.FV_STATUS='P' THEN 'Positive ' WHEN A.FV_STATUS='N' THEN 'Negative ' WHEN A.FV_STATUS='S' THEN 'Send Back ' WHEN A.FV_STATUS='R' THEN 'Refer ' END AS DECISION,B.AUTHOR_REMARKS AS REMARKS FROM CR_FIELD_VERIFICATION_DTL A
JOIN CR_DEAL_VERIFICATION_DTL B ON A.VERIFICATION_ID=B.VERIFICATION_ID AND A.DEAL_ID=B.DEAL_ID
JOIN SEC_USER_M C ON C.USER_ID=B.AUTHOR_ID
WHERE B.DEAL_ID=$P{p_deal_id}]]>
	</queryString>
