	<queryString>
		<![CDATA[SELECT IFNULL(DL.DEAL_NO,'NA')DEAL_NO,IFNULL(DL.DEAL_APPLICATION_FORM_NO,'NA')DEAL_APPLICATION_FORM_NO,
IFNULL(LON.ELIGIBLE_NHB_SCHEME,'NA')ELIGIBLE_NHB_SCHEME,IFNULL(IN_CAMEL_CASE(CPM.PRODUCT_DESC),'')PRODUCT,
(CASE LON.PROPERTY_CURRENT_STATUS WHEN 'BU' THEN 'BUILT UP ' WHEN 'UC' THEN 'UNDER CONSTRUCTION ' ELSE '' END)PROPERTY_CURRENT_STATUS_DESC,
CASE WHEN TYP.TYPE='HOME_PURCHASE' THEN CONCAT(IFNULL(IN_CAMEL_CASE(CPM.PRODUCT_DESC),''),if(ifnull(PROPERTY_CURRENT_STATUS,'')!='',' (',''),
(CASE PROPERTY_CURRENT_STATUS WHEN 'BU' THEN 'BUILT UP ' WHEN 'UC' THEN 'UNDER CONSTRUCTION ' ELSE '' END),
if(ifnull(PROPERTY_CURRENT_STATUS,'')!='',' )','')) ELSE IFNULL(IN_CAMEL_CASE(CPM.PRODUCT_DESC),'NA') END PRODUCT_DESC,
IFNULL(LIFE_INSURANCE,'N')LIFE_INSURANCE,IFNULL(PROPERTY_INSURANCE,'N')PROPERTY_INSURANCE,IFNULL(LOAN_PRODUCTS_INSURANCE ,'N')LOAN_PRODUCTS_INSURANCE,
IN_CAMEL_CASE(GM.DESCRIPTION)LOAN_TYPE,IN_CAMEL_CASE(GM1.DESCRIPTION)LOAN_SUB_TYPE,
IFNULL(CON_LOAN.DEAL_LOAN_AMOUNT,0.00)DEAL_LOAN_AMOUNT,IFNULL(LON.DEAL_TENURE,0)DEAL_TENURE,IFNULL(PROP.CT,0) ASSET_COUNT,
IFNULL(PROP.TOTAL_PURCHASE_PRICE,0.00)TOTAL_PURCHASE_PRICE,IFNULL(PROP.CONSTRUCTION_COST,0.00)CONSTRUCTION_COST,
IFNULL(PROP.AMOUNT_ALREADY_SPENT,0.00)AMOUNT_ALREADY_SPENT,IFNULL(PROP.BANK_SAVING,0.00)BANK_SAVING,IFNULL(PROP.OTHER_COST,0.00)OTHER_COST,
(IFNULL(PROP.TOTAL_PURCHASE_PRICE,0)+IFNULL(PROP.CONSTRUCTION_COST,0))REQUIRED_AMOUNT,
(IFNULL(CON_LOAN.DEAL_LOAN_AMOUNT,0)+IFNULL(AMOUNT_ALREADY_SPENT,0)+IFNULL(BANK_SAVING,0))SOURCE_AMOUNT,
IN_CAMEL_CASE(IFNULL(REG.REGION_DESC,''))REGION_DESC,IN_CAMEL_CASE(IFNULL(BR.BRANCH_DESC,''))BRANCH_DESC,
IN_CAMEL_CASE(IFNULL(DEAL_SOURCE_TYPE,''))DEAL_SOURCE_TYPE,IN_CAMEL_CASE(IFNULL(DEAL_SOURCE_NAME,''))DEAL_SOURCE_NAME,
IN_CAMEL_CASE(concat(IFNULL(RO.USER_NAME,''),'(',RO.USER_ID,')'))RO_NAME,
IN_CAMEL_CASE(concat(IFNULL(RM.USER_NAME,''),'(',RM.USER_ID,')'))RM_NAME,DATE_FORMAT(DEAL_INITIATION_DATE,'%d-%m-%y')DEAL_INITIATION_DATE,
IN_CAMEL_CASE(IFNULL(GCD.CUSTOMER_NAME,''))APPLICANT_NAME,IFNULL(COP.CO_APPLICANT_NAME,'')CO_APPLICANT_NAME,IFNULL(DL.LIFE_INSURANCE,'N')LIFE_INSURANCE
FROM CR_DEAL_DTL DL
LEFT JOIN GCD_CUSTOMER_M GCD ON(GCD.CUSTOMER_ID=DL.DEAL_CUSTOMER_ID)
LEFT JOIN(
	SELECT DEAL_ID,GROUP_CONCAT(IN_CAMEL_CASE(GCD.CUSTOMER_NAME) SEPARATOR '|')CO_APPLICANT_NAME
	FROM CR_DEAL_CUSTOMER_ROLE RL
	JOIN GCD_CUSTOMER_M GCD ON(RL.DEAL_CUSTOMER_ID=GCD.CUSTOMER_ID AND RL.DEAL_CUSTOMER_ROLE_TYPE='COAPPL')
	WHERE RL.DEAL_ID=$P{p_deal_id}
	GROUP BY RL.DEAL_ID
)COP ON(COP.DEAL_ID=DL.DEAL_ID)
JOIN CR_DEAL_LOAN_DTL LON ON(DL.DEAL_ID=LON.DEAL_ID AND LON.DEAL_LOAN_ID=(SELECT MIN(DEAL_LOAN_ID) FROM CR_DEAL_LOAN_DTL WHERE DEAL_ID=DL.DEAL_ID))
JOIN(
	SELECT T.DEAL_ID,SUM(T.DEAL_LOAN_AMOUNT)DEAL_LOAN_AMOUNT
	FROM CR_DEAL_LOAN_DTL T
	WHERE T.DEAL_ID=$P{p_deal_id}
	GROUP BY T.DEAL_ID
)CON_LOAN ON(DL.DEAL_ID=CON_LOAN.DEAL_ID)
LEFT JOIN GENERIC_MASTER GM ON(GM.GENERIC_KEY='LOAN_TYPE' AND GM.VALUE=LON.LOAN_TYPE)
LEFT JOIN GENERIC_MASTER GM1 ON(GM1.GENERIC_KEY='LOAN_SUB_TYPE' AND GM1.VALUE=LON.LOAN_SUB_TYPE)
LEFT JOIN stage_product_scheme_type TYP ON(TYP.PRODUCT_ID=LON.DEAL_PRODUCT)
JOIN CR_PRODUCT_M CPM ON(LON.DEAL_PRODUCT=CPM.PRODUCT_ID)
LEFT JOIN COM_BRANCH_M BR ON(BR.BRANCH_ID=DL.DEAL_BRANCH)
LEFT JOIN COM_REGION_M REG ON(REG.REGION_ID=BR.REGION_ID)
LEFT JOIN SEC_USER_M RO ON(RO.USER_ID=DL.DEAL_RO)
LEFT JOIN SEC_USER_M RM ON(RM.USER_ID=DL.DEAL_PM)
LEFT JOIN(
	SELECT COL.DEAL_ID,SUM(IFNULL(ASSET.TOTAL_PURCHASE_PRICE,0.00))TOTAL_PURCHASE_PRICE,
	SUM(IFNULL(ASSET.AMOUNT_ALREADY_SPENT,0.00))AMOUNT_ALREADY_SPENT,SUM(IFNULL(ASSET.CONSTRUCTION_COST,0.00))CONSTRUCTION_COST,
	SUM(IFNULL(ASSET.BANK_SAVING,0.00))BANK_SAVING,0.00 OTHER_COST,COUNT(1)CT
	FROM CR_DEAL_COLLATERAL_M  COL
	JOIN CR_ASSET_COLLATERAL_M ASSET ON(COL.ASSETID=ASSET.ASSET_ID)
	WHERE COL.DEAL_ID=$P{p_deal_id}
	GROUP BY COL.DEAL_ID
)PROP ON(PROP.DEAL_ID=DL.DEAL_ID)
WHERE DL.DEAL_ID=$P{p_deal_id} AND 'DEAL'=$P{p_stage}

UNION ALL

SELECT IFNULL(DL.LEAD_ID,'NA')DEAL_NO,IFNULL(DL.SALES_ID,'NA')DEAL_APPLICATION_FORM_NO,
IFNULL(DL.ELIGIBLE_NHB_SCHEME,'')ELIGIBLE_NHB_SCHEME,IFNULL(IN_CAMEL_CASE(CPM.PRODUCT_DESC),'')PRODUCT,
(CASE DL.PROPERTY_CURRENT_STATUS WHEN 'BU' THEN 'BUILT UP ' WHEN 'UC' THEN 'UNDER CONSTRUCTION ' ELSE '' END)PROPERTY_CURRENT_STATUS_DESC,
CASE WHEN TYP.TYPE='HOME_PURCHASE' THEN CONCAT(IFNULL(IN_CAMEL_CASE(CPM.PRODUCT_DESC),''),if(ifnull(PROPERTY_CURRENT_STATUS,'')!='',' (',''),
(CASE PROPERTY_CURRENT_STATUS WHEN 'BU' THEN 'BUILT UP ' WHEN 'UC' THEN 'UNDER CONSTRUCTION ' ELSE '' END),
if(ifnull(PROPERTY_CURRENT_STATUS,'')!='',' )','')) ELSE IFNULL(IN_CAMEL_CASE(CPM.PRODUCT_DESC),'NA') END PRODUCT_DESC,
IFNULL(LIFE_INSURANCE,'N')LIFE_INSURANCE,IFNULL(PROPERTY_INSURANCE,'N')PROPERTY_INSURANCE,IFNULL(LOAN_PRODUCTS_INSURANCE ,'N')LOAN_PRODUCTS_INSURANCE,
IN_CAMEL_CASE(GM.DESCRIPTION)LOAN_TYPE,IN_CAMEL_CASE(GM1.DESCRIPTION)LOAN_SUB_TYPE,
IFNULL(DL.AMOUNT_REQUIRED,0.00)DEAL_LOAN_AMOUNT,IFNULL(DL.LOAN_TENURE,0)DEAL_TENURE,IFNULL(PROP.CT,0) ASSET_COUNT,
IFNULL(PROP.TOTAL_PURCHASE_PRICE,0.00)TOTAL_PURCHASE_PRICE,IFNULL(PROP.CONSTRUCTION_COST,0.00)CONSTRUCTION_COST,
IFNULL(PROP.AMOUNT_ALREADY_SPENT,0.00)AMOUNT_ALREADY_SPENT,IFNULL(PROP.BANK_SAVING,0.00)BANK_SAVING,IFNULL(PROP.OTHER_COST,0.00)OTHER_COST,
(IFNULL(PROP.TOTAL_PURCHASE_PRICE,0)+IFNULL(PROP.CONSTRUCTION_COST,0))REQUIRED_AMOUNT,
(IFNULL(DL.AMOUNT_REQUIRED,0)+IFNULL(AMOUNT_ALREADY_SPENT,0)+IFNULL(BANK_SAVING,0))SOURCE_AMOUNT,
IN_CAMEL_CASE(IFNULL(REG.REGION_DESC,''))REGION_DESC,IN_CAMEL_CASE(IFNULL(BR.BRANCH_DESC,''))BRANCH_DESC,
IN_CAMEL_CASE(IFNULL(DEAL_SOURCE_TYPE,''))DEAL_SOURCE_TYPE,IN_CAMEL_CASE(IFNULL(DEAL_SOURCE_NAME,''))DEAL_SOURCE_NAME,
IN_CAMEL_CASE(CONCAT(IFNULL(RO.USER_NAME,''),'(',RO.USER_ID,')'))RO_NAME,
IN_CAMEL_CASE(CONCAT(IFNULL(RM.USER_NAME,''),'(',RM.USER_ID,')'))RM_NAME,DATE_FORMAT(DL.LEAD_GENERATION_DATE,'%d-%m-%y')DEAL_INITIATION_DATE,
IN_CAMEL_CASE(CONCAT(IFNULL(GCD.FIRST_NAME,''),' ',IFNULL(GCD.MIDDLE_NAME,''),' ',IFNULL(GCD.LAST_NAME,'')))APPLICANT_NAME,
IFNULL(COP.CO_APPLICANT_NAME,'')CO_APPLICANT_NAME,IFNULL(DL.LIFE_INSURANCE,'N')LIFE_INSURANCE
FROM STAGE_LEAD_DTL DL
LEFT JOIN STAGE_CUSTOMER_ROLE RL ON(DL.LEAD_ID=RL.SALES_LEAD_ID AND RL.CUSTOMER_ROLE='PRAPPL')
LEFT JOIN STAGE_CUSTOMER_DTL GCD ON (RL.CUSTOMER_ID=GCD.ID)
LEFT JOIN(
	SELECT RL.SALES_LEAD_ID,GROUP_CONCAT(IN_CAMEL_CASE(CONCAT(IFNULL(FIRST_NAME,''),' ',IFNULL(MIDDLE_NAME,''),' ',IFNULL(LAST_NAME,''))) SEPARATOR '|')CO_APPLICANT_NAME
	FROM STAGE_CUSTOMER_ROLE RL
	JOIN STAGE_CUSTOMER_DTL GCD ON (RL.CUSTOMER_ID=GCD.ID AND RL.CUSTOMER_ROLE='COAPPL')
	WHERE RL.SALES_LEAD_ID=$P{p_deal_id}
	GROUP BY RL.SALES_LEAD_ID
)COP ON(COP.SALES_LEAD_ID=DL.LEAD_ID)
JOIN CR_PRODUCT_M CPM ON(DL.PRODUCT=CPM.PRODUCT_ID)
LEFT JOIN stage_product_scheme_type TYP ON(TYP.PRODUCT_ID=DL.PRODUCT)
LEFT JOIN COM_BRANCH_M BR ON(BR.BRANCH_ID=DL.BRANCH_ID)
LEFT JOIN COM_REGION_M REG ON(REG.REGION_ID=BR.REGION_ID)
LEFT JOIN SEC_USER_M RO ON(RO.USER_ID=DL.RO_ID)
LEFT JOIN SEC_USER_M RM ON(RM.USER_ID=DL.LEAD_GENERATED_RM)
LEFT JOIN GENERIC_MASTER GM ON(GM.GENERIC_KEY='LOAN_TYPE' AND GM.VALUE=DL.LOAN_TYPE)
LEFT JOIN GENERIC_MASTER GM1 ON(GM1.GENERIC_KEY='LOAN_SUB_TYPE' AND GM1.VALUE=DL.LOAN_SUB_TYPE)
LEFT JOIN(
	SELECT ASSET.SALES_LEAD_ID DEAL_ID,SUM(IFNULL(ASSET.TOTAL_PURCHASE_PRICE,0.00))TOTAL_PURCHASE_PRICE,
	SUM(IFNULL(ASSET.AMOUNT_ALREADY_SPENT,0.00))AMOUNT_ALREADY_SPENT,SUM(IFNULL(ASSET.CONSTRUCTION_COST,0.00))CONSTRUCTION_COST,
	SUM(IFNULL(ASSET.BANK_SAVING,0.00))BANK_SAVING,0.00 OTHER_COST,COUNT(1)CT
	FROM STAGE_ASSET_COLLATERAL_DTL ASSET
	WHERE ASSET.SALES_LEAD_ID=$P{p_deal_id}
	GROUP BY ASSET.SALES_LEAD_ID
)PROP ON(PROP.DEAL_ID=DL.LEAD_ID)
WHERE DL.LEAD_ID=$P{p_deal_id} AND 'STAGE'=$P{p_stage}]]>
	</queryString>
