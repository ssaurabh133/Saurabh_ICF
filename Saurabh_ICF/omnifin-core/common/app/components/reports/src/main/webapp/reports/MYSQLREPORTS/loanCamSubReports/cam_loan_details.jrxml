	<parameter name="p_loan_id" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT distinct cr_loan_dtl.LOAN_ID,concat(cr_product_m.PRODUCT_DESC,'  ')as PRODUCT_DESC,
concat(cr_scheme_m.SCHEME_DESC,'  ')as SCHEME_DESC, cr_loan_dtl.LOAN_LOAN_AMOUNT,cr_loan_dtl.LOAN_TENURE,case when LOAN_RATE_TYPE='F'
 then 'Flat ' when LOAN_RATE_TYPE='P' then 'Ptpm ' when LOAN_RATE_TYPE='E' then 'Effective ' end as LOAN_RATE_TYPE,LOAN_FINAL_RATE,case when LOAN_INSTALLMENT_TYPE='E' then 'Equated ' when LOAN_INSTALLMENT_TYPE='S' then 'Structured ' end as LOAN_INSTALLMENT_TYPE,case when LOAN_REPAYMENT_TYPE='I' then 'Installment ' when LOAN_REPAYMENT_TYPE='N' then 'Non Installment ' end as LOAN_REPAYMENT_TYPE,
case when LOAN_REPAYMENT_FREQ='M' then 'Monthly ' when LOAN_REPAYMENT_FREQ='B' then 'Bi-Monthly ' when LOAN_REPAYMENT_FREQ='Q' then 'Quarterly ' when LOAN_REPAYMENT_FREQ='H' then 'Half Yearly ' when LOAN_REPAYMENT_FREQ='Y' then 'Yearly ' end as LOAN_REPAYMENT_FREQ,case when LOAN_RATE_METHOD='F' then 'Fixed ' when LOAN_RATE_METHOD='L' then 'Floating ' end as LOAN_RATE_METHOD,concat(LOAN_BASE_RATE_TYPE,' ')as LOAN_BASE_RATE_TYPE,LOAN_MARKUP,LOAN_NO_OF_INSTALLMENT,
SD_AMOUNT,case when SD_INTEREST_TYPE='N' then 'No Interest ' when SD_INTEREST_TYPE='S' then 'Simple Interest ' when SD_INTEREST_TYPE='C' then 'Compound Interest ' end as SD_INTEREST_TYPE ,SD_INTEREST_RATE,case when SD_COMPOUNDING_FREQ='M' then 'Monthly '  when SD_COMPOUNDING_FREQ='B' then 'Bi-Monthly '   when SD_COMPOUNDING_FREQ='Q' then 'Quarterly '   when SD_COMPOUNDING_FREQ='H' then 'Half Yearly '   when SD_COMPOUNDING_FREQ='Y' then 'Yearly ' end as SD_COMPOUNDING_FREQ,SD_TENURE,SD_INTEREST,case when SD_ADJUSTMENT='Y' then 'Yes ' when SD_ADJUSTMENT='N' then 'No ' end as SD_ADJUSTMENT,
case when DEAL_CHARGE_TYPE='P' then 'Payable ' when DEAL_CHARGE_TYPE='R' then 'Receivable ' end as DEAL_CHARGE_TYPE,
t.DEAL_CHARGE_CODE,
concat(c.CHARGE_DESC,' ')as CHARGE_DESC,
concat(GM.DESCRIPTION,' ')as DESCRIPTION,
concat(d.CUSTOMER_NAME,' ')as CUSTOMER_NAME,
case when DEAL_CHARGE_TAX_INCLUSIVE='N' then 'No ' when DEAL_CHARGE_TAX_INCLUSIVE='Y' then 'Yes ' end as DEAL_CHARGE_TAX_INCLUSIVE,
t.DEAL_CHARGE_TAX_RATE1,
t.DEAL_CHARGE_TAX_RATE2,
case when DEAL_CHARGE_METHOD='P' then 'Percentage ' when DEAL_CHARGE_METHOD='F' then 'Flat ' end as DEAL_CHARGE_METHOD,
t.DEAL_CHARGE_CALCULATED_AMOUNT,
t.DEAL_CHARGE_FINAL_AMOUNT
FROM cr_loan_dtl
join cr_product_m on(cr_product_m.PRODUCT_ID=cr_loan_dtl.LOAN_PRODUCT)
join cr_scheme_m on(cr_scheme_m.SCHEME_ID=cr_loan_dtl.LOAN_SCHEME)
left outer join  cr_sd_m on (cr_sd_m.TXN_ID=cr_loan_dtl.LOAN_ID)
left outer join cr_txncharges_dtl t on t.TXN_ID=cr_loan_dtl.LOAN_ID
left outer join com_charge_code_m c on t.DEAL_CHARGE_CODE=c.CHARGE_CODE
join gcd_customer_m d on d.CUSTOMER_ID=t.DEAL_CHARGE_BP_ID
left outer join generic_master GM on GM.VALUE=t.DEAL_CHARGE_BP_TYPE and GM.GENERIC_KEY='BPTYPE'
where   cr_loan_dtl.LOAN_ID=$P{p_loan_id}]]>
	</queryString>
