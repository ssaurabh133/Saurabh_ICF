	<parameter name="p_deal_id" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT DISTINCT C.CUSTOMER_ID,C.STAKEHOLDER_ID,CONCAT(C.STAKEHOLDER_TYPE,' ')STAKEHOLDER_TYPE,C.STAKEHOLDER_DOB,C.STAKEHOLDER_IDENTIFICATION_NO,C.STAKEHOLDER_POSITION,
if(C.ELIGIBLE_FOR_COMPUTATION='Y','YES ','NO ')ELIGIBLE_FOR_COMPUTATION,CONCAT(d.DESCRIPTION,' ')DESCRIPTION, CONCAT(C.STAKEHOLDER_NAME,' ')STAKEHOLDER_NAME,
C.STAKEHOLDER_JOINING_DATE, ifnull(STAKEHOLDER_PERCENTAGE,0)STAKEHOLDER_PERCENTAGE,CONCAT(STAKEHOLDER_PRIMARY_EMAIL,' ')STAKEHOLDER_PRIMARY_EMAIL,
CONCAT(STAKEHOLDER_WEBSITE,' ')STAKEHOLDER_WEBSITE
FROM CR_DEAL_DTL CDD
left join cr_loan_dtl cld on cld.loan_deal_id=cdd.DEAL_ID
left outer join cr_loan_customer_role clcr on(CLD.LOAN_ID=clcr.LOAN_ID)
LEFT OUTER JOIN gcd_customer_m gcm ON (CLD.LOAN_CUSTOMER_ID=gcm.CUSTOMER_ID)
LEFT OUTER JOIN customer_stakeholder_m C ON gcm.CUSTOMER_ID=C.CUSTOMER_ID
INNER join generic_master d on C.stakeholder_position=d.VALUE AND D.GENERIC_KEY='DESIGNATION'
WHERE CDD.DEAL_ID=$P{p_deal_id}
group by c.CUSTOMER_ID]]>
	</queryString>
