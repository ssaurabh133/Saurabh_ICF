	<queryString>
		<![CDATA[

-- final

Select LEDGER_ID as Ledger_ID,LEDGER_DESC  ,SUBLEDGER_ID as SL_CODE,SL_DESCRIPTION , sum(Op_DR) as OP_DR_Amount ,
sum(Op_Cr) as OP_CR_Amount ,  sum(DR_AMOUNT) as closingdebit , sum(CR_AMOUNT) as closingcredit,
sum(Curr_Dr) as CURR_DR_Amount ,  sum(Cur_cr) as CURR_CR_Amount
from
(
-- opening

select gl_ledger_m.LEDGER_ID , concat(gl_ledger_m.LEDGER_DESC,' ')LEDGER_DESC ,
 ifnull(gl_voucher_dtl.SUBLEDGER_ID,'') SUBLEDGER_ID,
 concat(ifnull(gl_sub_ledger.SL_DESCRIPTION,''),' ')SL_DESCRIPTION ,
if(sum(DR_AMOUNT) - sum(CR_AMOUNT) > 0 ,sum(DR_AMOUNT) - sum(CR_AMOUNT) , 0 ) as Op_DR,
if(sum(DR_AMOUNT) - sum(CR_AMOUNT) < 0 ,abs(sum(DR_AMOUNT) - sum(CR_AMOUNT)) , 0 ) as Op_Cr,
0 as Curr_Dr,
0 as Cur_cr,
0 as DR_Amount,
0 as CR_Amount

from
gl_voucher_dtl
 inner join gl_ledger_m on gl_voucher_dtl.Ledger_ID = gl_ledger_m.LEDGER_ID
left join gl_sub_ledger on gl_voucher_dtl.SUBLEDGER_ID = gl_sub_ledger.SL_CODE
left join (select concat('LN-',LOAN_ID) as SLCode , LOAN_REFERENCE_NO , LOAN_ID from cr_loan_dtl) loan_dtl
on gl_sub_ledger.SL_CODE = loan_dtl.SLCode
left join gl_group_m on gl_ledger_m.GROUP_ID = gl_group_m.GROUP_ID
inner join  (select com_branch_m.* from com_branch_m where
if((select sec_user_m.BRANCH_ACCESS  from sec_user_m where sec_user_m.USER_ID = $P{p_session_id})='A',com_branch_m.BRANCH_ID= com_branch_m.BRANCH_ID , com_branch_m.BRANCH_ID = '-098645')

union

select com_branch_m.* from com_branch_m inner join sec_user_branch_dtl on com_branch_m.BRANCH_ID = sec_user_branch_dtl.BRANCH_ID
and sec_user_branch_dtl.USER_ID = $P{p_session_id}
 ) com_branch_m on gl_voucher_dtl.branch_id = com_branch_m.BRANCH_ID
where gl_voucher_dtl.STATUS_ST = 'A' and if(gl_ledger_m.ACC_TYPE in ('I','E'), gl_voucher_dtl.VOUCHER_DATE >= GetYearStartingDate(1,STR_TO_DATE($P{p_from_date},'%d-%m-%Y'),'S') , gl_voucher_dtl.VOUCHER_DATE < STR_TO_DATE($P{p_from_date},'%d-%m-%Y'))
and if($P{p_country} = 0,gl_voucher_dtl.company_id =  1, com_branch_m.COUNTRY_ID = $P{p_country})
and if($P{p_state} = 0,gl_voucher_dtl.company_id =  1, com_branch_m.STATE_ID = $P{p_state})
and if($P{p_district} = 0,gl_voucher_dtl.company_id =  1, com_branch_m.DISTRICT_ID = $P{p_district})
and if($P{p_region} = 0, gl_voucher_dtl.company_id =  1, com_branch_m.REGION_ID = $P{p_region})
and if($P{p_branch} = '', gl_voucher_dtl.company_id =  1, gl_voucher_dtl.branch_id =  $P{p_branch})
and if($P{p_business}  = '',  gl_voucher_dtl.company_id =  1,  gl_voucher_dtl.module_id = $P{p_business})
and if($P{p_product} = '',  gl_voucher_dtl.company_id =  1, gl_voucher_dtl.product_id = $P{p_product} )
and if($P{p_costcentreheaders}= 0 , gl_voucher_dtl.company_id =  1,gl_voucher_dtl.department = $P{p_costcentreheaders})
and gl_ledger_m.SL_FLAG='Y'
and gl_ledger_m.LEDGER_ID=$P{p_accountNAme}

group by   gl_ledger_m.LEDGER_ID , gl_ledger_m.LEDGER_DESC ,
gl_group_m.GROUP_ID, gl_group_m.GROUP_DESC,
 gl_ledger_m.ACC_TYPE , ifnull(gl_voucher_dtl.SUBLEDGER_ID,'') ,
ifnull(loan_dtl.LOAN_REFERENCE_NO,'')  , ifnull(gl_sub_ledger.SL_DESCRIPTION,'')  ,
ifnull(loan_dtl.LOAN_ID,'')

having abs(sum(DR_AMOUNT) - sum(CR_AMOUNT)) > 0


union ALL

-- current
select gl_ledger_m.LEDGER_ID , concat(gl_ledger_m.LEDGER_DESC,' ')LEDGER_DESC  ,
 ifnull(gl_voucher_dtl.SUBLEDGER_ID,'') SUBLEDGER_ID,
 concat(ifnull(gl_sub_ledger.SL_DESCRIPTION,''),' ')SL_DESCRIPTION ,
0 as Op_DR,
0 as Op_Cr,
sum(DR_AMOUNT)  as Curr_Dr,
sum(CR_AMOUNT) as Cur_cr,
0 as DR_Amount ,
0 as CR_Amount

from
gl_voucher_dtl
 inner join gl_ledger_m on gl_voucher_dtl.Ledger_ID = gl_ledger_m.LEDGER_ID
left join gl_sub_ledger on gl_voucher_dtl.SUBLEDGER_ID = gl_sub_ledger.SL_CODE
left join (select concat('LN-',LOAN_ID) as SLCode , LOAN_REFERENCE_NO , LOAN_ID from cr_loan_dtl) loan_dtl
on gl_sub_ledger.SL_CODE = loan_dtl.SLCode
left join gl_group_m on gl_ledger_m.GROUP_ID = gl_group_m.GROUP_ID
inner join  (select com_branch_m.* from com_branch_m where
if((select sec_user_m.BRANCH_ACCESS  from sec_user_m where sec_user_m.USER_ID = $P{p_session_id})='A',com_branch_m.BRANCH_ID= com_branch_m.BRANCH_ID , com_branch_m.BRANCH_ID = '-098645')

union

select com_branch_m.* from com_branch_m inner join sec_user_branch_dtl on com_branch_m.BRANCH_ID = sec_user_branch_dtl.BRANCH_ID
and sec_user_branch_dtl.USER_ID = $P{p_session_id}
 ) com_branch_m on gl_voucher_dtl.branch_id = com_branch_m.BRANCH_ID
where gl_voucher_dtl.STATUS_ST = 'A' and gl_voucher_dtl.VOUCHER_DATE <=STR_TO_DATE($P{p_to_date},'%d-%m-%Y')

and gl_voucher_dtl.VOUCHER_DATE >=STR_TO_DATE($P{p_from_date},'%d-%m-%Y')
and if($P{p_country} = 0,gl_voucher_dtl.company_id =  1, com_branch_m.COUNTRY_ID = $P{p_country})
and if($P{p_state} = 0,gl_voucher_dtl.company_id =  1, com_branch_m.STATE_ID = $P{p_state})
and if($P{p_district} = 0,gl_voucher_dtl.company_id =  1, com_branch_m.DISTRICT_ID = $P{p_district})
and if($P{p_region} = 0, gl_voucher_dtl.company_id =  1, com_branch_m.REGION_ID = $P{p_region})
and if($P{p_branch} = '', gl_voucher_dtl.company_id =  1, gl_voucher_dtl.branch_id =  $P{p_branch})
and if($P{p_business}  = '',  gl_voucher_dtl.company_id =  1,  gl_voucher_dtl.module_id = $P{p_business})
and if($P{p_product} = '',  gl_voucher_dtl.company_id =  1, gl_voucher_dtl.product_id = $P{p_product} )
and if($P{p_costcentreheaders}= 0 , gl_voucher_dtl.company_id =  1,gl_voucher_dtl.department = $P{p_costcentreheaders})

and gl_ledger_m.SL_FLAG='Y'
and gl_ledger_m.LEDGER_ID=$P{p_accountNAme}

group by   gl_ledger_m.LEDGER_ID , gl_ledger_m.LEDGER_DESC ,
gl_group_m.GROUP_ID, gl_group_m.GROUP_DESC,
 gl_ledger_m.ACC_TYPE , ifnull(gl_voucher_dtl.SUBLEDGER_ID,'') ,
ifnull(loan_dtl.LOAN_REFERENCE_NO,'')  , ifnull(gl_sub_ledger.SL_DESCRIPTION,'')  ,
ifnull(loan_dtl.LOAN_ID,'')


-- closing

union all
select gl_ledger_m.LEDGER_ID , concat(gl_ledger_m.LEDGER_DESC,' ')LEDGER_DESC  ,
 ifnull(gl_voucher_dtl.SUBLEDGER_ID,'') SUBLEDGER_ID,
 concat(ifnull(gl_sub_ledger.SL_DESCRIPTION,''),' ')SL_DESCRIPTION ,
0 as Op_DR,
0 as Op_Cr,
0 as Curr_Dr,
0 as Cur_cr,
if(sum(DR_AMOUNT) - sum(CR_AMOUNT) > 0 ,sum(DR_AMOUNT) - sum(CR_AMOUNT) , 0 ) as DR_Amount,
if(sum(DR_AMOUNT) - sum(CR_AMOUNT) < 0 ,abs(sum(DR_AMOUNT) - sum(CR_AMOUNT)) , 0 ) as CR_Amount

from
gl_voucher_dtl
 inner join gl_ledger_m on gl_voucher_dtl.Ledger_ID = gl_ledger_m.LEDGER_ID
left join gl_sub_ledger on gl_voucher_dtl.SUBLEDGER_ID = gl_sub_ledger.SL_CODE
left join (select concat('LN-',LOAN_ID) as SLCode , LOAN_REFERENCE_NO , LOAN_ID from cr_loan_dtl) loan_dtl
on gl_sub_ledger.SL_CODE = loan_dtl.SLCode
left join gl_group_m on gl_ledger_m.GROUP_ID = gl_group_m.GROUP_ID
inner join  (select com_branch_m.* from com_branch_m where
if((select sec_user_m.BRANCH_ACCESS  from sec_user_m where sec_user_m.USER_ID = $P{p_session_id})='A',com_branch_m.BRANCH_ID= com_branch_m.BRANCH_ID , com_branch_m.BRANCH_ID = '-098645')

union

select com_branch_m.* from com_branch_m inner join sec_user_branch_dtl on com_branch_m.BRANCH_ID = sec_user_branch_dtl.BRANCH_ID
and sec_user_branch_dtl.USER_ID = $P{p_session_id}
 ) com_branch_m on gl_voucher_dtl.branch_id = com_branch_m.BRANCH_ID
where gl_voucher_dtl.STATUS_ST = 'A' and if(gl_ledger_m.ACC_TYPE in ('I','E'), gl_voucher_dtl.VOUCHER_DATE >= GetYearStartingDate(1,STR_TO_DATE($P{p_from_date},'%d-%m-%Y'),'S') , gl_voucher_dtl.VOUCHER_DATE <= STR_TO_DATE($P{p_to_date},'%d-%m-%Y'))
and if($P{p_country} = 0,gl_voucher_dtl.company_id =  1, com_branch_m.COUNTRY_ID = $P{p_country})
and if($P{p_state} = 0,gl_voucher_dtl.company_id =  1, com_branch_m.STATE_ID = $P{p_state})
and if($P{p_district} = 0,gl_voucher_dtl.company_id =  1, com_branch_m.DISTRICT_ID = $P{p_district})
and if($P{p_region} = 0, gl_voucher_dtl.company_id =  1, com_branch_m.REGION_ID = $P{p_region})
and if($P{p_branch} = '', gl_voucher_dtl.company_id =  1, gl_voucher_dtl.branch_id =  $P{p_branch})
and if($P{p_business}  = '',  gl_voucher_dtl.company_id =  1,  gl_voucher_dtl.module_id = $P{p_business})
and if($P{p_product} = '',  gl_voucher_dtl.company_id =  1, gl_voucher_dtl.product_id = $P{p_product} )
and if($P{p_costcentreheaders}= 0 , gl_voucher_dtl.company_id =  1,gl_voucher_dtl.department = $P{p_costcentreheaders})

and gl_ledger_m.SL_FLAG='Y'
and gl_ledger_m.LEDGER_ID=$P{p_accountNAme}

group by   gl_ledger_m.LEDGER_ID , gl_ledger_m.LEDGER_DESC ,
gl_group_m.GROUP_ID, gl_group_m.GROUP_DESC,
 gl_ledger_m.ACC_TYPE , ifnull(gl_voucher_dtl.SUBLEDGER_ID,'') ,
ifnull(loan_dtl.LOAN_REFERENCE_NO,'')  , ifnull(gl_sub_ledger.SL_DESCRIPTION,'')  ,
ifnull(loan_dtl.LOAN_ID,'')

having abs(sum(DR_AMOUNT) - sum(CR_AMOUNT)) > 0



) abc
group by Ledger_Desc,SL_CODE,Ledger_ID,LEDGER_DESC]]>
	</queryString>
