	<queryString>
		<![CDATA[select distinct cdd.deal_id, CDD.DEAL_CURRENT_APPROVAL_LEVEL,CONCAT(CDD.DEAL_APPLICATION_FORM_NO,' ')DEAL_APPLICATION_FORM_NO,CDD.LEAD_ID,CDD.DEAL_DATE,
replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(concat(ifnull(com_branch_m.BRANCH_DESC,''),' '),'!','! '),'%','% '),'(','( '),')',') '),'-','- '),'{','{ '),'[','[ '),'}','} '),']','] '),':',': '),';','; '),'"','" '),'/','/ '),'?','? '),'.','. '),',',', ')BRANCH_DESC,
CDD.DEAL_NO,
replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(concat(ifnull(CDCM.CUSTOMER_NAME,''),' '),'!','! '),'%','% '),'(','( '),')',') '),'-','- '),'{','{ '),'[','[ '),'}','} '),']','] '),':',': '),';','; '),'"','" '),'/','/ '),'?','? '),'.','. '),',',', ')Customer_name,
CDD.DEAL_INITIATION_DATE DEAL_ENCODED_DATE,
CONCAT(CDD.DEAL_SOURCE_TYPE,'   ')as DEAL_SOURCE_TYPE,
concat(CDD.DEAL_SOURCE_NAME,'   ')as DEAL_SOURCE_NAME,
case CDD.REC_STATUS
when 'P' then 'PENDING '
when 'X' then if((select count(1) from cr_deal_movement_dtl where REC_STATUS='A' and deal_id=CDD.DEAL_ID)>0,'REJECTED ','CANCELLED ')
when 'A' then 'APPROVED '
when 'F' then 'FORWARDED '
end as Status,
(SELECT CONCAT(CDD.MAKER_ID,' - ',USER_NAME,' ') FROM SEC_USER_M WHERE SEC_USER_M.USER_ID=CDD.MAKER_ID)MAKER_NAME,CDD.MAKER_DATE,CONCAT(CDCM.CUSTOMER_EMAIL,' ')CUSTOMER_EMAIL
,if(ADDRESS_LINE1 is null,if(CDAM.ADDRESS_LINE2 is null,if(CDAM.ADDRESS_LINE3 is null,' ',concat(CDAM.ADDRESS_LINE3,' ')),if(CDAM.ADDRESS_LINE3 is null,concat(CDAM.ADDRESS_LINE2,' '),concat(concat(CDAM.ADDRESS_LINE2,' , '),
concat(CDAM.ADDRESS_LINE3,' ')))),if(CDAM.ADDRESS_LINE2 is null,if(CDAM.ADDRESS_LINE3 is null,concat(ADDRESS_LINE1,' '),concat(concat(ADDRESS_LINE1,' , '),concat(CDAM.ADDRESS_LINE3,' '))),if(CDAM.ADDRESS_LINE3 is null,
concat(concat(ADDRESS_LINE1,' , '),concat(CDAM.ADDRESS_LINE2,' ')),concat(concat(concat(ADDRESS_LINE1,' , '),concat(CDAM.ADDRESS_LINE2,' , ')),concat(CDAM.ADDRESS_LINE3,' ')))))as Address,
replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(concat(ifnull(cr_product_m.PRODUCT_DESC,''),' '),'!','! '),'%','% '),'(','( '),')',') '),'-','- '),'{','{ '),'[','[ '),'}','} '),']','] '),':',': '),';','; '),'"','" '),'/','/ '),'?','? '),'.','. '),',',', ')PRODUCT_DESC,
replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(concat(ifnull(cr_scheme_m.SCHEME_DESC,''),' '),'!','! '),'%','% '),'(','( '),')',') '),'-','- '),'{','{ '),'[','[ '),'}','} '),']','] '),':',': '),';','; '),'"','" '),'/','/ '),'?','? '),'.','. '),',',', ')SCHEME_DESC,
CDAM.PRIMARY_PHONE,CDAM.ALTERNATE_PHONE,cr_deal_loan_dtl.DEAL_LOAN_AMOUNT,CDMD.user_name AUTHOR_NAME,GROUP_CONCAT(d.ASSET_COLLATERAL_DESC SEPARATOR '/')as VEHICLE_DESC,concat(CAPR.APPROVAL_REMARKS,' ')as REJECT_REMARK,concat(cdtd.AUTHOR_REMARKS,' ')as CANCEL_REMARK,
CDD.DEAL_SOURCE_TYPE as SOURCE_TYPE_DESC,CDD.DEAL_SOURCE_NAME as DSA_NAME,S.USER_NAME as RM_NAME,su.USER_NAME as RO_NAME,cr_deal_loan_dtl.DEAL_SANCTION_AMOUNT as SANCTION_AMOUNT,
min(cdad.approval_date) as FIRST_SANCTION_DATE

from
CR_DEAL_DTL CDD
join CR_DEAL_CUSTOMER_M CDCM on (CDD.DEAL_CUSTOMER_ID=CDCM.CUSTOMER_ID)
join com_branch_m on (CDD.DEAL_BRANCH=com_branch_m.BRANCH_ID)
join cr_deal_loan_dtl on cr_deal_loan_dtl.DEAL_ID=CDD.DEAL_ID
join cr_product_m on cr_product_m.PRODUCT_ID=cr_deal_loan_dtl.DEAL_PRODUCT
join cr_scheme_m on cr_scheme_m.SCHEME_ID=cr_deal_loan_dtl.DEAL_SCHEME
join CR_DEAL_ADDRESS_M CDAM on CDCM.CUSTOMER_ID=CDAM.BPID and BPTYPE='CS' and COMMUNICATION_ADDRESS='Y'
join sec_user_m S on S.USER_ID=CDD.DEAL_RM
left join cr_deal_approval_dtl cdad on cdad.deal_id=CDD.deal_id and cdad.APPROVAL_DECISION IN ('A','X')
left join sec_user_m su on su.user_id=CDD.DEAL_RO
left join cr_deal_termination_dtl cdtd on cdtd.DEAL_ID=CDD.DEAL_ID AND cdtd.REC_STATUS='A'
left join cr_deal_collateral_m c on CDD.DEAL_ID=c.DEAL_ID
left join cr_asset_collateral_m d on d.ASSET_ID=c.ASSETID
left join
(
	SELECT A.DEAL_ID,APPROVAL_REMARKS
	FROM cr_deal_approval_dtl A
	JOIN
	(
   		SELECT  DEAL_ID,MAX(DEAL_APPROVAL_ID) AS ID
    		from cr_deal_approval_dtl
    		WHERE APPROVAL_DECISION='X'
    		GROUP BY DEAL_ID
   	) B  ON A.DEAL_APPROVAL_ID=B.ID
)CAPR on (CAPR.DEAL_ID=CDD.DEAL_ID)
left join
(
	select a.deal_id,concat(c.user_name,' ')user_name
	from cr_deal_approval_dtl a
	join
	(
		select cr_deal_approval_dtl.deal_id,max(DEAL_APPROVAL_ID)id
		from cr_deal_approval_dtl
		JOIN CR_DEAL_DTL CDD ON CDD.DEAL_ID=cr_deal_approval_dtl.DEAL_ID
		AND (($P{p_p_date}='All' AND 'A'='A')or((CDD.DEAL_INITIATION_DATE>=STR_TO_DATE($P{p_date_from},'%Y-%m-%d %H:%i')))
			AND (CDD.DEAL_INITIATION_DATE <DATE_ADD(STR_TO_DATE($P{p_date_to},'%Y-%m-%d %H:%i') , INTERVAL 1 DAY)))
		AND  (($P{p_status}='All' AND  'A'='A') OR (CDD.REC_STATUS=$P{status}))
		AND  (($P{p_printed_deal}='All' AND  'A'='A') OR ((CDD.DEAL_ID >=$P{p_deal_from}) AND  (CDD.DEAL_ID <=$P{p_deal_to})))
		AND (($P{p_branch_type}='All' AND  'A'='A') OR (CDD.DEAL_BRANCH=$P{p_branch}))
		where approval_decision='A'
		group by cr_deal_approval_dtl.deal_id
	)b on(a.DEAL_APPROVAL_ID=b.id and a.deal_id=b.deal_id)
	join sec_user_m c on(c.user_id=a.approval_by)
	join cr_deal_dtl d on(d.deal_id=a.deal_id and d.rec_status='A')
)CDMD on(CDD.DEAL_ID=CDMD.DEAL_ID)
where
(($P{p_branch_type}='All' and CDD.DEAL_BRANCH in (SELECT BRANCH_ID FROM sec_user_branch_dtl where  user_id=  $P{p_user_id})) or (FIND_IN_SET(CDD.DEAL_BRANCH ,$P{p_branch})))
    and (($P{productCategory}='All' and 'A'='A' ) or (cr_deal_loan_dtl.deal_PRODUCT_CATEGORY =($P{productCategory})))
AND (($P{p_p_date}='All' AND 'A'='A')or((CDD.DEAL_INITIATION_DATE>=STR_TO_DATE($P{p_date_from},'%Y-%m-%d %H:%i')))
		AND (CDD.DEAL_INITIATION_DATE <STR_TO_DATE(DATE_ADD(STR_TO_DATE($P{p_date_to},'%Y-%m-%d %H:%i') , INTERVAL 1 DAY),'%Y-%m-%d %H:%i')))
AND  (($P{p_status}='All' AND  'A'='A') OR (CDD.REC_STATUS=$P{status}))
AND  (($P{p_printed_deal}='All' AND  'A'='A') OR ((CDD.DEAL_ID >=$P{p_deal_from}) AND  (CDD.DEAL_ID <=$P{p_deal_to})))
group by CDD.DEAL_ID
ORDER BY com_branch_m.branch_id,CDD.DEAL_ID]]>
