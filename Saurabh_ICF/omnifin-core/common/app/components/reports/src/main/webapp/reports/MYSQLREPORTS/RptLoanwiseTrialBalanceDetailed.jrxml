	<queryString>
		<![CDATA[select temp_trial_balance.BRANCH_ID as Reference_No ,
  temp_trial_balance.SL_CODE AS LOAN_NO ,CUSTOMER_NAME,
  temp_trial_balance.LEDGER_DESC ,temp_trial_balance.Ledger_ID AS LEDGER_ID ,
  temp_trial_balance.OP_DR_Amount as OP_DR_Amount,
  temp_trial_balance.OP_CR_Amount as OP_CR_Amount,
  CURR_DR_Amount as CURR_DR_Amount,
  CURR_CR_Amount  as CURR_CR_Amount,
  VOUCHER_DATE  ,VOUCHER_NO,
  if(temp_ledger.CR_AMOUNT > 0 , temp_ledger.CR_AMOUNT ,0) as LDebit ,
  if(temp_ledger.DR_AMOUNT > 0 , temp_ledger.DR_AMOUNT ,0) as LCredit,
(select PARAMETER_VALUE from parameter_mst where PARAMETER_KEY = 'APPLICATION_PATH') as AppPah

from temp_trial_balance
left join temp_ledger on
       temp_ledger.Session_ID = $P{p_session_id}
       and temp_ledger.Ledger_ID = temp_trial_balance.Ledger_ID
       and convert(temp_ledger.REFERENCE_NO,decimal) = temp_trial_balance.BRANCH_ID

where temp_trial_balance.Session_ID = $P{p_session_id}
ORDER by temp_trial_balance.BRANCH_ID ,temp_trial_balance.SL_CODE ,temp_trial_balance.CUSTOMER_NAME,
temp_trial_balance.LEDGER_DESC ,
temp_trial_balance.Ledger_ID , VOUCHER_DATE  ,mid(VOUCHER_NO,2)]]>
	</queryString>
