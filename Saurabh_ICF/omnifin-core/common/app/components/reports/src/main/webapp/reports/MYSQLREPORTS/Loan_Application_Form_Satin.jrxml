	<queryString>
		<![CDATA[SELECT CCM.ADDRESS_LINE1 AS OFFICE_ADDRESS_LINE1,
ifnull(CCM.ADDRESS_LINE2,'')AS OFFICE_ADDRESS_LINE2,
ifnull(CCM.ADDRESS_LINE3,'')AS OFFICE_ADDRESS_LINE3,
ifnull(CCM.CIN_NO,'')CIN_NO ,ifnull(CCM.EMAIL_ID,'')EMAIL_ID, ifnull(CDD.DEAL_APPLICATION_FORM_NO,'')APPLICATION_NO,
DATE_FORMAT(CDD.DEAL_INITIATION_DATE,'%d-%m-%y')DEAL_INITIATION_DATE,CDD.DEAL_CUSTOMER_ID
FROM CR_DEAL_DTL CDD
JOIN COM_COMPANY_M CCM ON(CCM.COMPANY_ID=1)
WHERE CDD.DEAL_ID=$P{p_deal_id} AND 'DEAL'=$P{p_stage}


UNION ALL

SELECT CCM.ADDRESS_LINE1 AS OFFICE_ADDRESS_LINE1,
ifnull(CCM.ADDRESS_LINE2,'')AS OFFICE_ADDRESS_LINE2,
ifnull(CCM.ADDRESS_LINE3,'')AS OFFICE_ADDRESS_LINE3,
ifnull(CCM.CIN_NO,'')CIN_NO ,ifnull(CCM.EMAIL_ID,'')EMAIL_ID, ifnull(SLD.SALES_ID,'')APPLICATION_NO,
DATE_FORMAT(SLD.LEAD_GENERATION_DATE,'%d-%m-%y')DEAL_INITIATION_DATE,CUSTOMER_ID AS DEAL_CUSTOMER_ID
FROM STAGE_LEAD_DTL SLD
LEFT JOIN STAGE_CUSTOMER_ROLE RL ON(SLD.LEAD_ID=RL.SALES_LEAD_ID AND RL.CUSTOMER_ROLE='PRAPPL')
LEFT JOIN STAGE_CUSTOMER_DTL GCD ON (RL.CUSTOMER_ID=GCD.ID)
JOIN COM_COMPANY_M CCM ON(CCM.COMPANY_ID=1)
WHERE SLD.LEAD_ID=$P{p_deal_id} AND 'STAGE'=$P{p_stage}]]>
	</queryString>
