	<queryString>
		<![CDATA[Select gl_bill_hdr.bill_gen_id,gl_bill_hdr.supplier_bill_no,if(gl_bill_hdr.bill_date='0000-00-00',null,gl_bill_hdr.bill_date)as bill_date,
concat(gl_party_dtl.PARTY_NAME, ' ') as supplier,if(gl_bill_hdr.due_date='0000-00-00',null,gl_bill_hdr.due_date)as due_date,
case when gl_bill_hdr.expense_type='R' then 'Receivable ' when gl_bill_hdr.expense_type='P'
then 'Payable ' end as expense_type,case when gl_bill_hdr.status='A' then 'Approved' when gl_bill_hdr.status='X' then 'Rejected' when
gl_bill_hdr.status='P' then 'Pending' when gl_bill_hdr.status='F' then 'Pending For Approval' end as status, case when gl_bill_hdr.bill_type='A' then 'Against Advance'when gl_bill_hdr.bill_type='B' then 'Against Bill' end as bill_type
from gl_bill_hdr
left outer join gl_party_dtl on gl_party_dtl.PARTY_ID = gl_bill_hdr.supplier_id
left join (select BILL_GEN_ID from gl_bill_cancellation where REC_STATUS<>'X')as r on gl_bill_hdr.bill_gen_id=r.BILL_GEN_ID
where  gl_bill_hdr.bill_date>=STR_TO_DATE($P{p_from_date},$P{p_date_format})
and gl_bill_hdr.bill_date<=STR_TO_DATE($P{p_to_date},$P{p_date_format})
and gl_bill_hdr.expense_type=$P{p_head_type}
and if($P{p_bill_no}='0',true,supplier_bill_no=$P{p_bill_no})
and if($P{p_suplier}='0',true,gl_party_dtl.PARTY_ID=$P{p_suplier})
and if($P{p_bill_type}='0',true,gl_bill_hdr.bill_type=$P{p_bill_type})
and gl_bill_hdr.status=$P{p_bill_status} and r.BILL_GEN_ID is null
order by gl_bill_hdr.bill_date]]>
	</queryString>
