	select loan_id,sum(ifnull(advice_amount ,0))roc_amt   -- 128

	from cr_txnadvice_dtl

	where charge_code_id=128 and advice_date>=str_to_date($P{p_p_date_from},'%d-%m-%Y') and  advice_date<=str_to_date($P{p_p_date_to},'%d-%m-%Y')

	group by loan_id

)roc on(roc.loan_id=A.loan_id)

left join

(

	select loan_id,group_concat((case INSURANCE_BY when 'B' then 'BORROWER ' when 'L' then 'LENDER ' else 'NA' end) SEPARATOR '|')INSURANCE_BY

	from cr_asset_insurance_dtl

	group by loan_id

)insu on(insu.loan_id=A.loan_id)

LEFT JOIN

(

SELECT CLD.LOAN_ID, CASE CLD.FUNDING_TYPE WHEN 'CP' THEN 'CONSORTIUM' WHEN 'OF' THEN 'OWN FUNDED' END AS FUNDING_TYPE,

IFNULL(CBPD1.LOAN_AMOUNT,0) AS SERVICE_PARTNER_LOAN_AMOUNT, IFNULL(CBPD2.LOAN_AMOUNT,0) AS LEAD_PARTNER_LOAN_AMOUNT

FROM CR_LOAN_DTL CLD

LEFT JOIN CR_BUSINESS_PARTNER_DTL CBPD1 ON (CBPD1.LOAN_ID = CLD.LOAN_ID AND CBPD1.SERVICE_PARTNER_FLAG='Y')

LEFT JOIN CR_BUSINESS_PARTNER_DTL CBPD2 ON (CBPD2.LOAN_ID = CLD.LOAN_ID AND CBPD2.SERVICE_PARTNER_FLAG='N')

)PARTNER_DTL ON (PARTNER_DTL.LOAN_ID = A.LOAN_ID)



LEFT JOIN

(

SELECT

A.LOAN_ID, SUM(IFNULL(CACM.ASSET_COLLATERAL_VALUE,0))ASSET_COLLATERAL_VALUE,

((IFNULL(A.LOAN_LOAN_AMOUNT,0)*100) / (SUM(IFNULL(CACM.ASSET_COLLATERAL_VALUE,0)))) AS NET_LTV,

GROUP_CONCAT(IFNULL(CACM.ASSET_COLLATERAL_CLASS,''),'  ')AS ASSET_COLLATERAL_DESC

FROM CR_LOAN_DTL A

LEFT JOIN cr_loan_collateral_m CLCM ON CLCM.LOAN_ID=A.LOAN_ID

LEFT JOIN cr_asset_collateral_m CACM ON CACM.ASSET_ID=CLCM.ASSETID

GROUP BY LOAN_ID

)ASSET ON (ASSET.LOAN_ID = A.LOAN_ID)

LEFT JOIN

(

SELECT A.loan_id,

GROUP_CONCAT(IFNULL(GCM2.CUSTOMER_NAME,''),' ')AS COAPPL_NAME

FROM  CR_LOAN_DTL A

LEFT JOIN CR_LOAN_CUSTOMER_ROLE CLCR1 ON CLCR1.LOAN_ID=A.LOAN_ID AND CLCR1.LOAN_CUSTOMER_ROLE_TYPE='COAPPL'

LEFT JOIN GCD_CUSTOMER_M GCM2 ON GCM2.CUSTOMER_ID=CLCR1.GCD_ID

group by a.loan_id

)COAPPL ON (COAPPL.LOAN_ID = A.LOAN_ID)



LEFT JOIN

(

SELECT

CLD.LOAN_ID, CLD.LOAN_PRODUCT, CDLD.DEAL_LOAN_ID, IFNULL(GM.DESCRIPTION,'') AS LOAN_PURPOSE

FROM CR_LOAN_DTL CLD

LEFT JOIN CR_DEAL_LOAN_DTL CDLD ON (CLD.LOAN_DEAL_LOAN_ID = CDLD.DEAL_LOAN_ID)

LEFT JOIN GENERIC_MASTER GM ON (GM.VALUE = CDLD.DEAL_LOAN_PURPOSE AND GM.PARENT_VALUE = CLD.LOAN_PRODUCT AND GM.GENERIC_KEY =  'LOAN_PURPOSE')

ORDER BY CLD.LOAN_ID

)LOAN ON (LOAN.LOAN_ID = A.LOAN_ID)



LEFT JOIN com_address_m CAM ON CAM.BPID=B.CUSTOMER_ID AND CAM.ADDRESS_TYPE in ('REI','PERMANENT') AND CAM.COMMUNICATION_ADDRESS='Y'

LEFT JOIN com_country_m Z ON Z.COUNTRY_ID=CAM.COUNTRY

LEFT JOIN COM_STATE_M CSM ON CSM.STATE_ID=CAM.STATE

LEFT JOIN com_district_m CDM ON CDM.DISTRICT_ID=CAM.DISTRICT

LEFT JOIN CR_LOAN_CUSTOMER_ROLE CLCR ON CLCR.LOAN_ID=A.LOAN_ID AND CLCR.LOAN_CUSTOMER_ROLE_TYPE='GUARANTOR'

LEFT JOIN GCD_CUSTOMER_M GCM1 ON GCM1.CUSTOMER_ID=CLCR.GCD_ID

LEFT JOIN com_address_m CAM1 ON CAM1.BPID=GCM1.CUSTOMER_ID AND CAM1.ADDRESS_TYPE in ('REI','PERMANENT') AND CAM1.COMMUNICATION_ADDRESS='Y'

LEFT JOIN com_country_m Z1 ON Z1.COUNTRY_ID=CAM1.COUNTRY

LEFT JOIN COM_STATE_M CSM1 ON CSM1.STATE_ID=CAM1.STATE

LEFT JOIN com_district_m CDM1 ON CDM1.DISTRICT_ID=CAM1.DISTRICT

LEFT JOIN CR_LOAN_DISBURSAL_DTL DISB ON DISB.LOAN_ID=A.LOAN_ID

left join cr_deal_address_m abc on CAM.ADDRESS_ID=abc.ADDRESS_ID

LEFT JOIN cr_deal_customer_m CDCMM ON CDCMM.CUSTOMER_ID=B.CUSTOMER_ID

left join (select loan_id,ifnull(INSTL_AMOUNT,0) LOAN_EMI_AMOUNT,min(instl_no) instl_no from cr_repaysch_dtl where BILL_FLAG='N'  group by loan_id) instAmount on a.loan_id = instAmount.loan_id

left join temp_loan_dump_scz on temp_loan_dump_scz.loan_id=A.loan_id
LEFT JOIN CR_CASE_MARKING_DTL CASE_ST ON CASE_ST.LOAN_ID=A.LOAN_ID AND CASE_STATUS IN ('RESTRUC','NONRESTRUC') AND CASE_ST.CASE_MARKING_FLAG='Y'


-- LEFT JOIN (SELECT loan_id,ifnull(INSTL_AMOUNT,0) LOAN_EMI_AMOUNT FROM CR_REPAYSCH_DTL WHERE LOAN_ID=A.LOAN_ID AND INSTL_NO IN (SELECT MAX(INSTL_NO)-1 FROM CR_REPAYSCH_DTL WHERE LOAN_ID=A.LOAN_ID AND REC_TYPE='I' GROUP BY LOAN_ID))

-- instAmount on a.loan_id = instAmount.loan_id

where
(($P{p_branch_type}='All' and A.loan_BRANCH in (SELECT BRANCH_ID FROM sec_user_branch_dtl where  user_id=  $P{p_user_id})) or (FIND_IN_SET(A.loan_BRANCH ,$P{p_branch})))
    and (($P{productCategory}='All' and 'A'='A' ) or (A.LOAN_PRODUCT_CATEGORY =($P{productCategory})))
    and (($P{loanClass}='All' and 'A'='A') or (A.sale_off_flag=$P{loanClass})) and A.LOAN_APPROVAL_DATE>=str_to_date($P{p_p_date_from},'%d-%m-%Y') and A.LOAN_APPROVAL_DATE<=str_to_date($P{p_p_date_to},'%d-%m-%Y')
group by A.loan_id]]>
	</queryString>
