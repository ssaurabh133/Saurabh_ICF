	<queryString>
		<![CDATA[select  temp_ledger.Session_ID , concat(gl_ledger_m.LEDGER_DESC,' ')as LEDGER_DESC  , temp_ledger.COMPANY_ID,temp_ledger.VOUCHER_NO , if (gl_voucher_dtl.DR_AMOUNT >0,'Debit' ,'Credit') as DebitCredit ,
if(gl_voucher_dtl.VOUCHER_TYPE ='P', 'Payment Voucher',
if(gl_voucher_dtl.VOUCHER_TYPE ='R', 'Receipt Voucher', if(gl_voucher_dtl.VOUCHER_TYPE ='J' , 'Journal Voucher',if(gl_voucher_dtl.VOUCHER_TYPE ='C' , 'Contra Voucher' , '' ))) )  as VoucherType,
gl_voucher_dtl.VOUCHER_DATE  , gl_voucher_dtl.VALUE_DATE  ,gl_voucher_dtl.MODULE_ID,concat(
(Select MODULE_DESCRIPTION from com_module_m where com_module_m.MODULE_ID=gl_voucher_dtl.MODULE_ID),' ')as MODULE_DESCRIPTION,gl_voucher_dtl.PRODUCT_ID,
concat((Select PRODUCT_DESCRIPTION from com_product_m where com_product_m.PRODUCT_ID=gl_voucher_dtl.PRODUCT_ID),' ')as PRODUCT_DESCRIPTION,
concat((Select NARRATION from com_product_stage_master   where com_product_stage_master.STAGE_ID=gl_voucher_dtl.STAGE_ID),' ')as STAGE,concat((Select BRANCH_DESC FROM com_branch_m  WHERE com_branch_m.BRANCH_ID=gl_voucher_dtl.BRANCH_ID),' ') as BRANCH_ID,
if(gl_voucher_dtl.DR_AMOUNT > 0  ,gl_voucher_dtl.DR_AMOUNT , gl_voucher_dtl.CR_AMOUNT) as Amount ,gl_voucher_dtl.DR_AMOUNT, gl_voucher_dtl.CR_AMOUNT  ,gl_voucher_dtl.LEDGER_ID ,gl_voucher_dtl.SUBLEDGER_ID ,gl_voucher_dtl.SEQUENCE_NO ,gl_voucher_dtl.LEDGER_ID,concat((select SL_DESCRIPTION from gl_sub_ledger where
 gl_sub_ledger.SL_CODE=gl_voucher_dtl.SUBLEDGER_ID),' ')as SL_DESCRIPTION,concat(gl_voucher_dtl.NARRATION,' ')as  NARRATION  , concat(gl_voucher_dtl.REFERENCE_NO,' ')as REFERENCE_NO ,concat(gl_voucher_dtl.cheque_invoice_no,' ')as cheque_invoice_no ,concat(gl_voucher_dtl.MAKER_ID,' ')as MAKER_ID,gl_voucher_dtl.MAKER_DATE,
case when gl_voucher_dtl.STATUS_ST='A' then 'Approved' when gl_voucher_dtl.STATUS_ST='R' then 'Rejected' when gl_voucher_dtl.STATUS_ST='F' then 'Pendig for Approval/Rejection' when gl_voucher_dtl.STATUS_ST='P' then 'Pending'end as STATUS, concat((Select USER_NAME from sec_user_m where sec_user_m.USER_ID=gl_voucher_dtl.AUTHOR_ID),' ')as AUTHOR_NAME ,gl_voucher_dtl.AUTHOR_DATE,
concat('Narration : ',gl_voucher_dtl.NARRATION) as Narration,
(select PARAMETER_VALUE from parameter_mst where PARAMETER_KEY = 'APPLICATION_PATH') as AppPah,
(Select DEPARTMENT_DESC from com_department_m where com_department_m.DEPARTMENT_ID=gl_voucher_dtl.department) as DEPARTMENT_DESC,
case when gl_voucher_dtl.Manual_Voucher='Y' then 'Manual Voucher' when gl_voucher_dtl.Manual_Voucher='N' then 'Auto Voucher' end as Manual_Voucher,
(select PARAMETER_VALUE from parameter_mst WHERE PARAMETER_KEY='GLVOUCHER_REPORT_DATE') AS date_display
from
((temp_ledger  inner join gl_voucher_dtl on temp_ledger.COMPANY_ID  = gl_voucher_dtl.COMPANY_ID and temp_ledger.VOUCHER_NO  = gl_voucher_dtl.VOUCHER_NO)
-- inner join gl_voucher_hdr on temp_ledger.COMPANY_ID  = gl_voucher_hdr.COMPANY_ID and temp_ledger.VOUCHER_NO  = gl_voucher_hdr.VOUCHER_NO
)
inner join gl_ledger_m
on gl_voucher_dtl.LEDGER_ID = gl_ledger_m.LEDGER_ID
 and gl_voucher_dtl.COMPANY_ID = gl_ledger_m.COMPANY_ID
where temp_ledger.Session_ID  = $P{p_sessionID} and temp_ledger.Ledger_ID=1
order by temp_ledger.VOUCHER_NO]]>
