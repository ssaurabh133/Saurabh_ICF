where $P{p_as_on_date}  between date(START_DATE) and date(END_DATE)) As a on (true)
where temp_fixed_asset_register.User_ID = $P{p_user_id} and Current_Branch=(case when $P{p_branchid}=0 then  Current_Branch else $P{p_branchid} end)
