	<parameter name="P_country_id" class="java.lang.String"/>
	<parameter name="P_district_id" class="java.lang.String"/>
	<parameter name="P_region_id" class="java.lang.String"/>
	<parameter name="P_branch_id" class="java.lang.String"/>
	<parameter name="P_department_id" class="java.lang.String"/>
	<parameter name="p_from_date" class="java.lang.String"/>
	<parameter name="p_to_date" class="java.lang.String"/>
	<parameter name="p_report_type" class="java.lang.String"/>
	<parameter name="p_vertical_desc" class="java.lang.String"/>
	<queryString>
		<![CDATA[
select a.*, b.* , com_branch_m.BRANCH_DESC as BRANCH_DESC ,
if(isnull(planned.PlannedAmount),0,planned.PlannedAmount) as PlannedAmount   , if(isnull(if(a.ACC_TYPE = 'Income' , Actual.Total_Cr - Actual.Total_Dr ,
Actual.Total_Dr - Actual.Total_Cr)) , 0 ,if(a.ACC_TYPE = 'Income' , Actual.Total_Cr - Actual.Total_Dr ,
Actual.Total_Dr - Actual.Total_Cr))   as Actual
from
(Select budget_master_id as p_budget_master_id, if(ACC_TYPE = 'I' , 'Income','Expense') as p_ACC_TYPE, parent_budget_master_id,
budget_code as p_budget_code, Budget_Desc as p_Budget_Desc , COMPANY_ID

from listofbudgetheads where parent_budget_master_id = 0 and COMPANY_ID = $P{p_company_id}) b

inner join
(select budget_master_id, budget_code, if(ACC_TYPE = 'I' , 'Income','Expense') as ACC_TYPE ,listofbudgetheads.Budget_Desc ,

    listofbudgetheads.parent_budget_master_id
   from listofbudgetheads
where listofbudgetheads.parent_budget_master_id > 0
and listofbudgetheads.COMPANY_ID = $P{p_company_id}
group by budget_master_id, budget_code, ACC_TYPE ,listofbudgetheads.Budget_Desc) a
on b.p_budget_master_id = a.parent_budget_master_id
inner join
com_branch_m
on  b.COMPANY_ID = com_branch_m.COMPANY_ID
and com_branch_m.COUNTRY_ID  = if($P{P_country_id} = '0', com_branch_m.COUNTRY_ID ,convert($P{P_country_id},decimal))
and com_branch_m.STATE_ID  = if( $P{p_state_id}= '0', com_branch_m.STATE_ID ,convert($P{p_state_id},decimal))
and com_branch_m.DISTRICT_ID  = if($P{P_district_id} = '0', com_branch_m.DISTRICT_ID ,convert($P{P_district_id},decimal))
and com_branch_m.REGION_ID  = if($P{P_region_id}= '0', com_branch_m.REGION_ID ,convert($P{P_region_id},decimal))
and com_branch_m.BRANCH_ID  = if($P{P_branch_id} = '', com_branch_m.BRANCH_ID ,convert($P{P_branch_id},decimal))

inner join com_country_m on com_branch_m.COUNTRY_ID = com_country_m.COUNTRY_ID
inner join com_state_m on com_branch_m.state_ID = com_state_m.state_id
inner join com_district_m on com_branch_m.district_ID = com_district_m.district_id
inner join com_region_m on com_branch_m.region_ID = com_region_m.region_id

left join
(
select BUDGET_GROUP_CODE ,
if($P{p_report_type}='B', gl_budget_definition.BRANCH_ID,if($P{p_report_type}='C',com_branch_m.COUNTRY_ID,if($P{p_report_type}='S',com_branch_m.STATE_ID,if($P{p_report_type}='D',com_branch_m.DISTRICT_ID,com_branch_m.REGION_ID))))
 as BRANCH_ID
,

sum(AMOUNT) as PlannedAmount
from gl_budget_definition inner join gl_accounting_period_m
on gl_budget_definition.PERIOD_ID = gl_accounting_period_m.PERIOD_ID
and gl_budget_definition.Vertical_Id = if($P{p_vertical_desc} = '0', gl_budget_definition.Vertical_Id ,convert($P{p_vertical_desc},decimal))
inner join com_branch_m on gl_budget_definition.BRANCH_ID = com_branch_m.BRANCH_ID
 where gl_budget_definition.COMPANY_ID = $P{p_company_id}
  and gl_accounting_period_m.START_DATE >= date_Add(LAST_DAY(DATE_ADD($P{p_from_date},INTERVAL 	-1 MONTH)),interval 1 day)
  and gl_accounting_period_m.START_DATE <= last_day($P{p_to_date})
  and gl_budget_definition.BRANCH_ID  = if($P{P_branch_id} = '', gl_budget_definition.BRANCH_ID ,convert($P{P_branch_id},decimal))
and com_branch_m.COUNTRY_ID  = if($P{P_country_id} = '0', com_branch_m.COUNTRY_ID ,convert($P{P_country_id},decimal))
and com_branch_m.STATE_ID  = if( $P{p_state_id}= '0', com_branch_m.STATE_ID ,convert($P{p_state_id},decimal))
and com_branch_m.DISTRICT_ID  = if($P{P_district_id} = '0', com_branch_m.DISTRICT_ID ,convert($P{P_district_id},decimal))
and com_branch_m.REGION_ID  = if($P{P_region_id}= '0', com_branch_m.REGION_ID ,convert($P{P_region_id},decimal))
and gl_budget_definition.department = if($P{P_department_id} = '',gl_budget_definition.department,convert($P{P_department_id},decimal))

 group by BUDGET_GROUP_CODE , if($P{p_report_type}='B', gl_budget_definition.BRANCH_ID,if($P{p_report_type}='C',com_branch_m.COUNTRY_ID,if($P{p_report_type}='S',com_branch_m.STATE_ID,if($P{p_report_type}='D',com_branch_m.DISTRICT_ID,com_branch_m.REGION_ID))))




) planned
on a.budget_master_id = planned.BUDGET_GROUP_CODE and
  com_branch_m.BRANCH_ID = planned.BRANCH_ID

left join
(
select BudgetID , Branch_id,
sum(subq.TotalDr) as Total_Dr , sum(subq.TotalCr) as Total_Cr
from
(
select if(ifnull(gl_voucher_dtl.budget_master_id ,0)=0, gl_budget_m.budget_master_id,gl_voucher_dtl.budget_master_id) as BudgetID,
gl_voucher_dtl.Branch_id  as Branch_id,
sum(gl_voucher_dtl.DR_AMOUNT * if(gl_voucher_dtl.YearClosing_Voucher = 'Y' ,-1,1)) as TotalDr ,
sum(gl_voucher_dtl.CR_AMOUNT * if(gl_voucher_dtl.YearClosing_Voucher = 'Y' ,-1,1)) as TotalCr
from (((gl_budget_m inner join gl_budget_group_ledger_map on gl_budget_m.budget_master_id = gl_budget_group_ledger_map.BUDGET_GROUP_CODE)
left join gl_voucher_dtl on gl_budget_group_ledger_map.LEDGER_ID = gl_voucher_dtl.LEDGER_ID and gl_budget_group_ledger_map.COMPANY_ID = gl_voucher_dtl.COMPANY_ID)

)
inner join com_branch_m v_com_branch on gl_voucher_dtl.branch_id = v_com_branch.branch_id
where gl_voucher_dtl.STATUS_ST = 'A'  and gl_voucher_dtl.voucher_date >=  date_Add(LAST_DAY(DATE_ADD($P{p_from_date},INTERVAL -1 MONTH)),interval 1 day)
and gl_voucher_dtl.voucher_date <= last_day($P{p_to_date})
and gl_voucher_dtl.branch_id <> (select PARAMETER_VALUE from parameter_mst where PARAMETER_KEY = 'HOBRANCHCODE')
and gl_voucher_dtl.YEAR_ID_IN = $P{p_fin_year_id}
and gl_voucher_dtl.BRANCH_ID  = if($P{P_branch_id} = '', gl_voucher_dtl.BRANCH_ID ,convert($P{P_branch_id},decimal))
and v_com_branch.COUNTRY_ID  = if($P{P_country_id} = '0', v_com_branch.COUNTRY_ID ,convert($P{P_country_id},decimal))
and v_com_branch.STATE_ID  = if( $P{p_state_id}= '0', v_com_branch.STATE_ID ,convert($P{p_state_id},decimal))
and v_com_branch.DISTRICT_ID  = if($P{P_district_id} = '0', v_com_branch.DISTRICT_ID ,convert($P{P_district_id},decimal))
and v_com_branch.REGION_ID  = if($P{P_region_id}= '0', v_com_branch.REGION_ID ,convert($P{P_region_id},decimal))
and gl_voucher_dtl.department = if($P{P_department_id} = '',gl_voucher_dtl.department,convert($P{P_department_id},decimal))
group by if(ifnull(gl_voucher_dtl.budget_master_id ,0)=0, gl_budget_m.budget_master_id,gl_voucher_dtl.budget_master_id), gl_voucher_dtl.Branch_id   ) subq
group by BudgetID , Branch_Id

union all

(select
if(ifnull(gl_voucher_hdr_dtl.budget_master_id ,0)=0, gl_budget_m.budget_master_id,gl_voucher_hdr_dtl.budget_master_id) as BudgetID,
gl_voucher_hdr_dtl.BRANCH_ID  as Branch_id,
sum(gl_voucher_hdr_dtl.DR_AMOUNT * if(gl_voucher_hdr_dtl.YearClosing_Voucher = 'Y' ,-1,1)) as TotalDr ,
sum(gl_voucher_hdr_dtl.CR_AMOUNT * if(gl_voucher_hdr_dtl.YearClosing_Voucher = 'Y' ,-1,1)) as TotalCr

from gl_budget_m inner join gl_budget_group_ledger_map on gl_budget_m.budget_master_id = gl_budget_group_ledger_map.BUDGET_GROUP_CODE
left join
(select
gl_ho_expense_ratio_dtl.BRANCH_ID as  BRANCH_ID,
gl_voucher_dtl.COMPANY_ID, gl_voucher_dtl.VOUCHER_NO, gl_voucher_dtl.VOUCHER_TYPE, gl_voucher_dtl.VOUCHER_DATE,  YEAR_ID_IN, PERIOD_ID_IN,
MODULE_ID
, department, STATUS_ST,
 PRODUCT_ID,  off_balance,YearClosing_Voucher

, gl_ho_expense_ratio_dtl.RATIO  , gl_voucher_dtl.SEQUENCE_NO , gl_voucher_dtl.LEDGER_ID ,
round((gl_voucher_dtl.DR_AMOUNT * gl_ho_expense_ratio_dtl.RATIO)/100) as DR_AMOUNT  ,
round((gl_voucher_dtl.CR_AMOUNT * gl_ho_expense_ratio_dtl.RATIO/100)) as CR_AMOUNT ,
gl_voucher_dtl.budget_master_id
from
gl_voucher_dtl
 inner join
(select distinct voucher_date , (select max(WITH_EFFECTIVE_FROM) from gl_ho_expense_ratio_hdr where WITH_EFFECTIVE_FROM <= gl_voucher_dtl.VOUCHER_DATE) as ratiodate
 from gl_voucher_dtl
where branch_id = (select PARAMETER_VALUE from parameter_mst where PARAMETER_KEY = 'HOBRANCHCODE')) abc
on gl_voucher_dtl.VOUCHER_DATE = abc.voucher_date
inner join gl_ho_expense_ratio_hdr on ratiodate = gl_ho_expense_ratio_hdr.WITH_EFFECTIVE_FROM
inner join gl_ho_expense_ratio_dtl on gl_ho_expense_ratio_hdr.HO_EXPENSE_ID = gl_ho_expense_ratio_dtl.HO_EXPENSE_ID
-- inner join gl_voucher_dtl on gl_voucher_hdr.VOUCHER_NO = gl_voucher_dtl.VOUCHER_NO
	-- and gl_voucher_hdr.COMPANY_ID = gl_voucher_dtl.COMPANY_ID
inner join com_branch_m v_com_branch on gl_voucher_dtl.branch_id = v_com_branch.branch_id
where
gl_voucher_dtl.STATUS_ST = 'A'
and gl_voucher_dtl.branch_id = (select PARAMETER_VALUE from parameter_mst where PARAMETER_KEY = 'HOBRANCHCODE')
and gl_voucher_dtl.YEAR_ID_IN = $P{p_fin_year_id}
and gl_voucher_dtl.BRANCH_ID  = if($P{P_branch_id} = '', gl_voucher_dtl.BRANCH_ID ,convert($P{P_branch_id},decimal))
and v_com_branch.COUNTRY_ID  = if($P{P_country_id} = '0', v_com_branch.COUNTRY_ID ,convert($P{P_country_id},decimal))
and v_com_branch.STATE_ID  = if( $P{p_state_id}= '0', v_com_branch.STATE_ID ,convert($P{p_state_id},decimal))
and v_com_branch.DISTRICT_ID  = if($P{P_district_id} = '0', v_com_branch.DISTRICT_ID ,convert($P{P_district_id},decimal))
and v_com_branch.REGION_ID  = if($P{P_region_id}= '0', v_com_branch.REGION_ID ,convert($P{P_region_id},decimal))
and gl_voucher_dtl.department = if($P{P_department_id} = '',gl_voucher_dtl.department,convert($P{P_department_id},decimal))




) gl_voucher_hdr_dtl
on gl_budget_group_ledger_map.LEDGER_ID = gl_voucher_hdr_dtl.LEDGER_ID and gl_budget_group_ledger_map.COMPANY_ID = gl_voucher_hdr_dtl.COMPANY_ID
group by if(ifnull(gl_voucher_hdr_dtl.budget_master_id ,0)=0, gl_budget_m.budget_master_id,gl_voucher_hdr_dtl.budget_master_id) ,
gl_voucher_hdr_dtl.BRANCH_ID
)
)  Actual
on
a.budget_master_id = Actual.BudgetID and
  com_branch_m.BRANCH_ID = Actual.Branch_ID]]>
	</queryString>
