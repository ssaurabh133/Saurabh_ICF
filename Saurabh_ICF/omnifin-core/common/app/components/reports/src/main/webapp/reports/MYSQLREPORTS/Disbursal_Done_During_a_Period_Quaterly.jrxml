	<queryString>
		<![CDATA[SELECT distinct CLD.LOAN_REFERENCE_NO,CLD.LOAN_NO,
replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(concat(ifnull(com_branch_m.BRANCH_DESC,''),' '),'!','! '),'%','% '),'(','( '),')',') '),'-','- '),'{','{ '),'[','[ '),'}','} '),']','] '),':',': '),';','; '),'"','" '),'/','/ '),'?','? '),'.','. '),',',', ')BRANCH_DESC,
replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(concat(ifnull(gcd_customer_m.CUSTOMER_NAME,''),' '),'!','! '),'%','% '),'(','( '),')',') '),'-','- '),'{','{ '),'[','[ '),'}','} '),']','] '),':',': '),';','; '),'"','" '),'/','/ '),'?','? '),'.','. '),',',', ')name,
replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(concat(ifnull(cr_product_m.PRODUCT_DESC,''),' '),'!','! '),'%','% '),'(','( '),')',') '),'-','- '),'{','{ '),'[','[ '),'}','} '),']','] '),':',': '),';','; '),'"','" '),'/','/ '),'?','? '),'.','. '),',',', ')product,
replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(concat(ifnull(cr_scheme_m.SCHEME_DESC,''),' '),'!','! '),'%','% '),'(','( '),')',') '),'-','- '),'{','{ '),'[','[ '),'}','} '),']','] '),':',': '),';','; '),'"','" '),'/','/ '),'?','? '),'.','. '),',',', ')scheme,
replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(concat(ifnull(com_industry_m.INDUSTRY_DESC,''),' '),'!','! '),'%','% '),'(','( '),')',') '),'-','- '),'{','{ '),'[','[ '),'}','} '),']','] '),':',': '),';','; '),'"','" '),'/','/ '),'?','? '),'.','. '),',',', ')INDUSTRY_DESC,
replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(replace(concat(ifnull(com_sub_industry_m.SUB_INDUSTRY_DESC,''),' '),'!','! '),'%','% '),'(','( '),')',') '),'-','- '),'{','{ '),'[','[ '),'}','} '),']','] '),':',': '),';','; '),'"','" '),'/','/ '),'?','? '),'.','. '),',',', ')SUB_INDUSTRY_DESC,
CONCAT(generic_master.DESCRIPTION,' ')as LOAN_SECTOR_TYPE,CLD.LOAN_APPROVAL_DATE, CLD.LOAN_LOAN_AMOUNT,   CLDD.DISBURSAL_DATE,
CASE CLD.LOAN_REPAYMENT_MODE
WHEN 'O' THEN 'OTC '
WHEN 'P' THEN 'PDC '
WHEN 'D' THEN 'DIRECT DEBIT '
WHEN 'E' THEN 'ECS '
WHEN 'H' THEN 'ACH '
END AS LOAN_REPAYMENT_MODE,CLDD.DISBURSAL_AMOUNT,
cld.Loan_Branch,
IFNULL((SELECT CONCAT(CLDD.MAKER_ID,' - ',USER_NAME,' ') FROM SEC_USER_M WHERE CLDD.MAKER_ID=SEC_USER_M.USER_ID),'MIGRATION ') MAKER_NAME,
IFNULL((SELECT CONCAT(CLDD.AUTHOR_ID,' - ',USER_NAME,' ') FROM SEC_USER_M WHERE CLDD.AUTHOR_ID=SEC_USER_M.USER_ID),'MIGRATION ') AUTHOR_NAME,
IF(CLDD.DISBURSAL_Stage='ADDDISB','ADDITIONAL DISBURSAL',IF(CLDD.DISBURSAL_FLAG='F','FULL ','PARTIAL '))DISBURSAL_FLAG
,CLD.LOAN_EFF_RATE,
IF(CLDD.DISBURSAL_Stage='ADDDISB' || (IFNULL(CLDD.SHORT_AMOUNT_ADJUSTED,0) = 0) ,0.00, (select (sum(if(CTDT.advice_type='R',CTDT.ADVICE_AMOUNT,0))- sum(if(CTDT.advice_type='P',CTDT.ADVICE_AMOUNT,0))) as ADVICE_AMOUNT
from cr_txnadvice_dtl CTDT
WHERE CTDT.LOAN_ID=CLD.LOAN_ID AND CTDT.CHARGE_CODE_ID=106 ))ADVICE_AMOUNT,
CLDD.SHORT_AMOUNT_ADJUSTED,CLDD.DISBURSAL_NO,IFNULL(SM.USER_NAME,'') AS DEAL_RO, IFNULL(UM.USER_NAME,'') AS DEAL_PM
-- EDC.DEAL_RO,EDC.DEAL_PM
,CSM.STATE_DESC, (IFNULL(CLD.LOAN_LOAN_AMOUNT,0)- (select sum(IFNULL(DISBURSAL_AMOUNT,0)) from cr_loan_disbursal_dtl where loan_id = cld.loan_id and Rec_status = 'A' group by loan_id))  as  DISB_PEND_AMT
from  cr_loan_dtl CLD
LEFT JOIN cr_deal_dtl EDC ON EDC.DEAL_ID=CLD.LOAN_DEAL_ID
join com_branch_m on(CLD.LOAN_BRANCH=com_branch_m.BRANCH_ID)
JOIN COM_STATE_M CSM ON (CSM.STATE_ID = com_branch_m.STATE_ID)
join gcd_customer_m on (CLD.LOAN_CUSTOMER_ID=gcd_customer_m.CUSTOMER_ID)
join cr_product_m on(CLD.LOAN_PRODUCT=cr_product_m.PRODUCT_ID)
join cr_scheme_m on(CLD.LOAN_SCHEME=cr_scheme_m.SCHEME_ID)
--  LEFT JOIN cr_txnadvice_dtl CTD ON CTD.LOAN_ID=CLD.LOAN_ID AND CTD.CHARGE_CODE_ID=106
LEFT OUTER JOIN com_industry_m ON(gcd_customer_m.CUSTOMER_INDUSTRY=com_industry_m.INDUSTRY_ID)
LEFT OUTER JOIN com_sub_industry_m ON(gcd_customer_m.CUSTOMER_SUB_INDUSTRY=com_sub_industry_m.sub_industry_id)
join cr_loan_disbursal_dtl CLDD on(CLD.LOAN_ID=CLDD.LOAN_ID and CLDD.REC_STATUS='A')
left join generic_master on CLD.LOAN_SECTOR_TYPE=generic_master.VALUE and generic_master.GENERIC_KEY='SECTOR_TYPE'
LEFT JOIN SEC_USER_M UM ON UM.USER_ID=EDC.DEAL_PM
LEFT JOIN SEC_USER_M SM ON SM.USER_ID=EDC.DEAL_RO
where
IFNULL(CLD.REC_STATUS,'P')NOT IN('X','L')
AND (($P{p_branch_type}='All' and CLD.loan_BRANCH in (SELECT BRANCH_ID FROM sec_user_branch_dtl where  user_id=  $P{p_user_id})) or (FIND_IN_SET(CLD.loan_BRANCH ,$P{p_branch})))
AND (($P{p_loan_type}='A' AND 'A'='A')OR(CLD.LOAN_id>=$P{p_loan_from})AND(CLD.LOAN_id<=$P{p_loan_to}))
AND (($P{p_p_date}='All' and 'A'='A')or((CLDD.DISBURSAL_DATE>=STR_TO_DATE($P{p_date_from},'%d-%m-%Y %H:%i')))
AND (CLDD.DISBURSAL_DATE <DATE_ADD(STR_TO_DATE($P{p_date_to},'%d-%m-%Y %H:%i') , INTERVAL 1 DAY)))
    and (($P{productCategory}='All' and 'A'='A' ) or (CLD.LOAN_PRODUCT_CATEGORY =($P{productCategory})))
    and (($P{loanClass}='All' and 'A'='A') or (CLD.sale_off_flag=$P{loanClass}))
order by CLD.LOAN_id desc ,CLDD.DISBURSAL_DATE,CLDD.DISBURSAL_NO]]>
	</queryString>
