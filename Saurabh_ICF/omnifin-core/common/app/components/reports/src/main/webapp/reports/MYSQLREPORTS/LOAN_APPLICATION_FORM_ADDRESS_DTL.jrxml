IN_CAMEL_CASE(CUR_RES.DESCRIPTION) CURRENT_RESIDENCE,CDAM.NO_OF_YEARS,IFNULL(CDAM.MONTHLY_RENT,0)MONTHLY_RENT,IFNULL(CUST.EMAIL,'') CUSTOMER_EMAIL,
IN_CAMEL_CASE(RES_OCC.DESCRIPTION) RESIDENCE_OCCUPIED
FROM STAGE_CUSTOMER_DTL CUST
LEFT JOIN stage_address_dtl CDAM ON(CUST.ID=CDAM.CUSTOMER_ID AND CDAM.ADDRESS_TYPE=$P{p_address_type})
LEFT JOIN COM_COUNTRY_M CCM ON(CCM.COUNTRY_ID=CDAM.COUNTRY)
LEFT JOIN COM_STATE_M CSM ON (CSM.STATE_ID=CDAM.STATE)
LEFT JOIN COM_DISTRICT_M CDM ON(CDM.DISTRICT_ID=CDAM.DISTRICT)
