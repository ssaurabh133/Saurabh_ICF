CDAM.PINCODE,IFNULL(CDAM.STD_CODE,'')STD_CODE,IFNULL(CDAM.ALTERNATE_PHONE,'')TELEPHONE,IFNULL(CDAM.PRIMARY_PHONE,'')MOBILE,
IN_CAMEL_CASE(CUR_RES.DESCRIPTION) CURRENT_RESIDENCE,CDAM.NO_OF_YEARS,IFNULL(CDAM.MONTHLY_RENT,0)MONTHLY_RENT,IFNULL(CUST.CUSTOMER_EMAIL,'')CUSTOMER_EMAIL,
IN_CAMEL_CASE(RES_OCC.DESCRIPTION) RESIDENCE_OCCUPIED
FROM CR_DEAL_CUSTOMER_M CUST
LEFT JOIN CR_DEAL_ADDRESS_M CDAM ON(CUST.CUSTOMER_ID=CDAM.BPID AND ADDRESS_TYPE=$P{p_address_type})
LEFT JOIN COM_COUNTRY_M CCM ON(CCM.COUNTRY_ID=CDAM.COUNTRY)
LEFT JOIN COM_STATE_M CSM ON (CSM.STATE_ID=CDAM.STATE)
LEFT JOIN COM_DISTRICT_M CDM ON(CDM.DISTRICT_ID=CDAM.DISTRICT)
LEFT JOIN COM_TEHSIL_M CTM ON (CTM.ID=CDAM.TAHSIL)
LEFT JOIN GENERIC_MASTER CUR_RES ON (CUR_RES.GENERIC_KEY='CURRENT_RESIDENCE' AND CUR_RES.VALUE=CDAM.ADDRESS_DETAIL)
LEFT JOIN GENERIC_MASTER RES_OCC ON (RES_OCC.GENERIC_KEY='RESIDENCE_OCCUPIED' AND RES_OCC.VALUE=CDAM.RESIDENCE_OCCUPIED)
WHERE CUST.CUSTOMER_ID=$P{P_CUSTOMER_ID} AND 'DEAL'=$P{p_stage} and CUST.CUSTOMER_TYPE='I'
union all
SELECT CASE WHEN CDAM.ADDRESS_TYPE='REI' THEN 'Current  Address ' ELSE 'Permanent  Address ' END ADDRESS_TYPE,
@AST_ADDRESS_DTL:=IFNULL(CDAM.PLOT_NO,'')ADD_PLOT_NO,
@AST_ADDRESS_DTL:=CASE WHEN (CDAM.FLOOR_NO IS NULL OR TRIM(CDAM.FLOOR_NO)='') THEN @AST_ADDRESS_DTL ELSE (CASE WHEN @AST_ADDRESS_DTL='' THEN CDAM.FLOOR_NO ELSE CONCAT(@AST_ADDRESS_DTL,', ',CDAM.FLOOR_NO) END) END ADD_FLOOR_NO,
