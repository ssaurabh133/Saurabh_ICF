IN_CAMEL_CASE(CASE WHEN (CDAM.ADDRESS_LINE3 IS NULL OR TRIM(CDAM.ADDRESS_LINE3)='')THEN @AST_ADDRESS_DTL ELSE (CASE WHEN @AST_ADDRESS_DTL='' THEN CDAM.ADDRESS_LINE3 ELSE CONCAT(@AST_ADDRESS_DTL,', ',CDAM.ADDRESS_LINE3) END) END) ADDRESS,
IN_CAMEL_CASE(CDAM.LANDMARK)LANDMARK,CCM.COUNTRY_ID,IN_CAMEL_CASE(CCM.COUNTRY_DESC)COUNTRY_DESC,CSM.STATE_ID,IN_CAMEL_CASE(CSM.STATE_DESC)STATE_DESC,
