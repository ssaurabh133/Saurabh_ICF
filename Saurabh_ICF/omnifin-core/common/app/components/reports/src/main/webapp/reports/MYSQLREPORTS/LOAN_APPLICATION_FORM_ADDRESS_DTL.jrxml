@AST_ADDRESS_DTL:=IFNULL(CDAM.PLOT_NO,'')ADD_PLOT_NO,
@AST_ADDRESS_DTL:=CASE WHEN (CDAM.FLOOR_NO IS NULL OR TRIM(CDAM.FLOOR_NO)='') THEN @AST_ADDRESS_DTL ELSE (CASE WHEN @AST_ADDRESS_DTL='' THEN CDAM.FLOOR_NO ELSE CONCAT(@AST_ADDRESS_DTL,', ',CDAM.FLOOR_NO) END) END ADD_FLOOR_NO,
@AST_ADDRESS_DTL:=CASE WHEN (CDAM.ADDRESS_LINE1 IS NULL OR TRIM(CDAM.ADDRESS_LINE1)='') THEN @AST_ADDRESS_DTL ELSE (CASE WHEN @AST_ADDRESS_DTL='' THEN CDAM.ADDRESS_LINE1 ELSE CONCAT(@AST_ADDRESS_DTL,', ',CDAM.ADDRESS_LINE1) END) END ADD_LINE0,
@AST_ADDRESS_DTL:=CASE WHEN (CDAM.ADDRESS_LINE2 IS NULL OR TRIM(CDAM.ADDRESS_LINE2)='') THEN @AST_ADDRESS_DTL ELSE (CASE WHEN @AST_ADDRESS_DTL='' THEN CDAM.ADDRESS_LINE2 ELSE CONCAT(@AST_ADDRESS_DTL,', ',CDAM.ADDRESS_LINE2) END) END ADD_LINE1,
IN_CAMEL_CASE(CASE WHEN (CDAM.ADDRESS_LINE3 IS NULL OR TRIM(CDAM.ADDRESS_LINE3)='')THEN @AST_ADDRESS_DTL ELSE (CASE WHEN @AST_ADDRESS_DTL='' THEN CDAM.ADDRESS_LINE3 ELSE CONCAT(@AST_ADDRESS_DTL,', ',CDAM.ADDRESS_LINE3) END) END) ADDRESS,
IN_CAMEL_CASE(CDAM.LANDMARK)LANDMARK,CCM.COUNTRY_ID,IN_CAMEL_CASE(CCM.COUNTRY_DESC)COUNTRY_DESC,CSM.STATE_ID,IN_CAMEL_CASE(CSM.STATE_DESC)STATE_DESC,
