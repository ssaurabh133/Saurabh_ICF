IN_CAMEL_CASE(RES_OCC.DESCRIPTION) RESIDENCE_OCCUPIED
FROM CR_DEAL_CUSTOMER_M CUST
LEFT JOIN CR_DEAL_ADDRESS_M CDAM ON(CUST.CUSTOMER_ID=CDAM.BPID AND ADDRESS_TYPE=if(ADDRESS_TYPE='OFFICE' and 'REI'=$P{p_address_type},'OFFICE',$P{p_address_type}))
